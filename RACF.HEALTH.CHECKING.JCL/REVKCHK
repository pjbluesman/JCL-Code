SET CURRENT SQLID = 'LSEBDB2';                                                  SELECT 'SYSUSERAUTH',CHAR(GRANTOR,8),                                                                CHAR(GRANTEE,8),DATEGRANTED,SYSADMAUTH,                     SYSADMAUTH,SYSOPRAUTH,SYSCTRLAUTH FROM SYSIBM.SYSUSERAUTH                      WHERE GRANTOR IN('T001237') OR                                                        GRANTEE IN('T001237') AND                                                           (SYSADMAUTH IN('Y','G') OR SYSOPRAUTH IN('Y','G')  OR                            SYSCTRLAUTH IN('Y','G'));                                            SELECT 'SYSDBAUTH',CHAR(GRANTOR,8),                                                                CHAR(GRANTEE,8),CHAR(NAME,20) AS DBNAME,                                        DATEGRANTED                                                  FROM SYSIBM.SYSDBAUTH                                                           WHERE GRANTOR IN('T001237');                                                    SELECT 'SYSTABAUTH',CHAR(GRANTOR,8),CHAR(GRANTEE,8),                                                CHAR(TCREATOR,15),CHAR(TTNAME,15),DATEGRANTED               FROM SYSIBM.SYSTABAUTH                                                          WHERE GRANTOR IN('T001237') ;                                                   SELECT 'SYSPLANAUTH',CHAR(GRANTOR,8),CHAR(GRANTEE,8),                                                CHAR(NAME,20) AS PLANNAME,DATEGRANTED                      FROM SYSIBM.SYSPLANAUTH                                                         WHERE GRANTOR IN('T001237');                                                    SELECT 'SYSCOLAUTH',CHAR(GRANTOR,8),CHAR(GRANTEE,8),                                                CHAR(COLNAME,20),DATEGRANTED                                FROM SYSIBM.SYSCOLAUTH                                                          WHERE GRANTOR IN('T001237');                                                    SELECT 'SYSRESAUTH',CHAR(GRANTOR,8),CHAR(GRANTEE,8),                                                CHAR(NAME,15) AS RESNAME,DATEGRANTED                        FROM SYSIBM.SYSRESAUTH                                                          WHERE GRANTOR IN('T001237');                                                    SELECT 'SYSROUTINEAUTH',CHAR(GRANTOR,8),CHAR(GRANTEE,8),                                                CHAR(SCHEMA,15),CHAR(SPECIFICNAME,15),                                          CHAR(EXECUTEAUTH,5) AS EXECUTE                          FROM SYSIBM.SYSROUTINEAUTH                                                      WHERE GRANTOR IN('T001237');                                                    SELECT 'SYSSCHEMAAUTH',CHAR(GRANTOR,8),CHAR(GRANTEE,8),                                                CHAR(SCHEMANAME,15)                                      FROM SYSIBM.SYSSCHEMAAUTH                                                       WHERE GRANTOR IN('T001237');                                                    SELECT 'SYSPACKAUTH',CHAR(GRANTOR,8),CHAR(GRANTEE,8),                                                CHAR(NAME,15),CHAR(COLLID,15),TIMESTAMP                    FROM SYSIBM.SYSPACKAUTH                                                         WHERE GRANTOR IN('T001237');                                                    SELECT 'SYSDATABASE',CHAR(CREATOR,8),CHAR(NAME,10),TIMESTAMP                    FROM SYSIBM.SYSDATABASE                                                         WHERE CREATOR IN('T001237');                                                    SELECT 'SYSTABLES',CHAR(CREATOR,8),CHAR(NAME,20),                                                  CHAR(DBNAME,10),CHAR(TSNAME,15)                              FROM SYSIBM.SYSTABLES                                                           WHERE CREATOR IN('T001237');                                                    SELECT 'SYSTABLESPACE',CHAR(CREATOR,8),CHAR(NAME,15),                                                  CHAR(DBNAME,10),STATSTIME                                FROM SYSIBM.SYSTABLESPACE                                                       WHERE CREATOR IN('T001237');                                                    SELECT 'SYSPLAN',NAME,CHAR(CREATOR,8),CHAR(BOUNDBY,10),BINDDATE                 FROM SYSIBM.SYSPLAN                                                             WHERE CREATOR IN('T001237') OR                                                        BOUNDBY IN('T001237');                                                    SELECT 'SYSVIEWS',CHAR(NAME,15) AS VIEWNAME,CHAR(CREATOR,10)                    FROM SYSIBM.SYSVIEWS                                                            WHERE CREATOR IN('T001237');                                                    SELECT 'SYSPACKAGE',CHAR(CREATOR,8),CHAR(NAME,20) AS PACKG_NAME,                                    CHAR(COLLID,15),BINDTIME                                    FROM SYSIBM.SYSPACKAGE                                                          WHERE CREATOR IN('T001237');                                                    SELECT 'SYSROUTINES',CHAR(SCHEMA,15),CHAR(OWNER,10),                                                 CHAR(NAME,20),ROUTINETYPE,CHAR(CREATEDBY,8)                FROM SYSIBM.SYSROUTINES                                                         WHERE OWNER IN('T001237');                                                      SELECT * FROM SYSIBM.SYSPLANDEP                                                 WHERE BCREATOR IN('T001237');                                                   