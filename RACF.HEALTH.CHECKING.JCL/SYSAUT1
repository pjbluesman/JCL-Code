 SELECT CHAR(GRANTEE,8),                                                00010000            DATEGRANTED,SYSADMAUTH                                      00020000                FROM SYSIBM.SYSUSERAUTH WHERE                           00030000    (SYSADMAUTH = 'Y' OR                                                00040000     SYSADMAUTH = 'G')                                                  00050000     ORDER BY 1,2;                                                      00060000