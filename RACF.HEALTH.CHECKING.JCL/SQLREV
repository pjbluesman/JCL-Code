SET CURRENT SQLID = 'LSEBDB2';                                                  SELECT 'SYSUSERAUTH',CHAR(GRANTOR,8),                                                                CHAR(GRANTEE,8),DATEGRANTED,SYSADMAUTH,                     SYSADMAUTH,SYSOPRAUTH,SYSCTRLAUTH FROM SYSIBM.SYSUSERAUTH                      WHERE GRANTOR IN('A729204','A837065','D66179T','D70052T','D72913T',                              'D85267T','D92833T','D95834T','T105570','TQ00235',                              'TQ0061T','TQ0519','WKRDBAM5','X66179C','YAA014',                               'YAA148','YCG005T','YDB001','YTL109','YWK001')  OR                   GRANTEE IN('A729204','A837065','D66179T','D70052T','D72913T',                              'D85267T','D92833T','D95834T','T105570','TQ00235',                              'TQ0061T','TQ0519','WKRDBAM5','X66179C','YAA014',                               'YAA148','YCG005T','YDB001','YTL109','YWK001')                       AND (SYSADMAUTH IN('Y','G') OR SYSOPRAUTH IN('Y','G')  OR                            SYSCTRLAUTH IN('Y','G'));                                            SELECT 'SYSDBAUTH',CHAR(GRANTOR,8),                                                                CHAR(GRANTEE,8),CHAR(NAME,20) AS DBNAME,                                        DATEGRANTED                                                  FROM SYSIBM.SYSDBAUTH                                                           WHERE GRANTOR IN('A729204','A837065','D66179T','D70052T','D72913T',                              'D85267T','D92833T','D95834T','T105570','TQ00235',                              'TQ0061T','TQ0519','WKRDBAM5','X66179C','YAA014',                               'YAA148','YCG005T','YDB001','YTL109','YWK001') ;               SELECT 'SYSTABAUTH',CHAR(GRANTOR,8),CHAR(GRANTEE,8),                                                CHAR(TCREATOR,15),CHAR(TTNAME,15),DATEGRANTED               FROM SYSIBM.SYSTABAUTH                                                          WHERE GRANTOR IN('A729204','A837065','D66179T','D70052T','D72913T',                              'D85267T','D92833T','D95834T','T105570','TQ00235',                              'TQ0061T','TQ0519','WKRDBAM5','X66179C','YAA014',                               'YAA148','YCG005T','YDB001','YTL109','YWK001') ;               SELECT 'SYSPLANAUTH',CHAR(GRANTOR,8),CHAR(GRANTEE,8),                                                CHAR(NAME,20) AS PLANNAME,DATEGRANTED                      FROM SYSIBM.SYSPLANAUTH                                                         WHERE GRANTOR IN('A729204','A837065','D66179T','D70052T','D72913T',                              'D85267T','D92833T','D95834T','T105570','TQ00235',                              'TQ0061T','TQ0519','WKRDBAM5','X66179C','YAA014',                               'YAA148','YCG005T','YDB001','YTL109','YWK001') ;               SELECT 'SYSCOLAUTH',CHAR(GRANTOR,8),CHAR(GRANTEE,8),                                                CHAR(COLNAME,20),DATEGRANTED                                FROM SYSIBM.SYSCOLAUTH                                                          WHERE GRANTOR IN('A729204','A837065','D66179T','D70052T','D72913T',                              'D85267T','D92833T','D95834T','T105570','TQ00235',                              'TQ0061T','TQ0519','WKRDBAM5','X66179C','YAA014',                               'YAA148','YCG005T','YDB001','YTL109','YWK001') ;               SELECT 'SYSRESAUTH',CHAR(GRANTOR,8),CHAR(GRANTEE,8),                                                CHAR(NAME,15) AS RESNAME,DATEGRANTED                        FROM SYSIBM.SYSRESAUTH                                                          WHERE GRANTOR IN ('A729204','A837065','D66179T','D70052T','D72913T',                             'D85267T','D92833T','D95834T','T105570','TQ00235',                              'TQ0061T','TQ0519','WKRDBAM5','X66179C','YAA014',                               'YAA148','YCG005T','YDB001','YTL109','YWK001');                SELECT 'SYSROUTINEAUTH',CHAR(GRANTOR,8),CHAR(GRANTEE,8),                                                CHAR(SCHEMA,15),CHAR(SPECIFICNAME,15),                                          CHAR(EXECUTEAUTH,5) AS EXECUTE                          FROM SYSIBM.SYSROUTINEAUTH                                                      WHERE GRANTOR IN ('A729204','A837065','D66179T','D70052T','D72913T',                             'D85267T','D92833T','D95834T','T105570','TQ00235',                              'TQ0061T','TQ0519','WKRDBAM5','X66179C','YAA014',                               'YAA148','YCG005T','YDB001','YTL109','YWK001');                SELECT 'SYSSCHEMAAUTH',CHAR(GRANTOR,8),CHAR(GRANTEE,8),                                                CHAR(SCHEMANAME,15)                                      FROM SYSIBM.SYSSCHEMAAUTH                                                       WHERE GRANTOR IN ('A729204','A837065','D66179T','D70052T','D72913T',                             'D85267T','D92833T','D95834T','T105570','TQ00235',                              'TQ0061T','TQ0519','WKRDBAM5','X66179C','YAA014',                               'YAA148','YCG005T','YDB001','YTL109','YWK001');                SELECT 'SYSPACKAUTH',CHAR(GRANTOR,8),CHAR(GRANTEE,8),                                                CHAR(NAME,15),CHAR(COLLID,15),TIMESTAMP                    FROM SYSIBM.SYSPACKAUTH                                                         WHERE GRANTOR IN ('A729204','A837065','D66179T','D70052T','D72913T',                             'D85267T','D92833T','D95834T','T105570','TQ00235',                              'TQ0061T','TQ0519','WKRDBAM5','X66179C','YAA014',                               'YAA148','YCG005T','YDB001','YTL109','YWK001');                SELECT 'SYSDATABASE',CHAR(CREATOR,8),CHAR(NAME,10),TIMESTAMP                    FROM SYSIBM.SYSDATABASE                                                         WHERE CREATOR IN('A729204','A837065','D66179T','D70052T','D72913T',                              'D85267T','D92833T','D95834T','T105570','TQ00235',                              'TQ0061T','TQ0519','WKRDBAM5','X66179C','YAA014',                               'YAA148','YCG005T','YDB001','YTL109','YWK001');                SELECT 'SYSTABLES',CHAR(CREATOR,8),CHAR(NAME,20),                                                  CHAR(DBNAME,10),CHAR(TSNAME,15)                              FROM SYSIBM.SYSTABLES                                                           WHERE CREATOR IN('A729204','A837065','D66179T','D70052T','D72913T',                              'D85267T','D92833T','D95834T','T105570','TQ00235',                              'TQ0061T','TQ0519','WKRDBAM5','X66179C','YAA014',                               'YAA148','YCG005T','YDB001','YTL109','YWK001');                SELECT 'SYSTABLESPACE',CHAR(CREATOR,8),CHAR(NAME,15),                                                  CHAR(DBNAME,10),STATSTIME                                FROM SYSIBM.SYSTABLESPACE                                                       WHERE CREATOR IN('A729204','A837065','D66179T','D70052T','D72913T',                              'D85267T','D92833T','D95834T','T105570','TQ00235',                              'TQ0061T','TQ0519','WKRDBAM5','X66179C','YAA014',                               'YAA148','YCG005T','YDB001','YTL109','YWK001');                SELECT 'SYSPLAN',NAME,CHAR(CREATOR,8),CHAR(BOUNDBY,10),BINDDATE                 FROM SYSIBM.SYSPLAN                                                             WHERE CREATOR IN('A729204','A837065','D66179T','D70052T','D72913T',                              'D85267T','D92833T','D95834T','T105570','TQ00235',                              'TQ0061T','TQ0519','WKRDBAM5','X66179C','YAA014',                               'YAA148','YCG005T','YDB001','YTL109','YWK001')  OR                   BOUNDBY IN('A729204','A837065','D66179T','D70052T','D72913T',                              'D85267T','D92833T','D95834T','T105570','TQ00235',                              'TQ0061T','TQ0519','WKRDBAM5','X66179C','YAA014',                               'YAA148','YCG005T','YDB001','YTL109','YWK001');                SELECT 'SYSVIEWS',CHAR(NAME,15) AS VIEWNAME,CHAR(CREATOR,10)                    FROM SYSIBM.SYSVIEWS                                                            WHERE CREATOR IN('A729204','A837065','D66179T','D70052T','D72913T',                              'D85267T','D92833T','D95834T','T105570','TQ00235',                              'TQ0061T','TQ0519','WKRDBAM5','X66179C','YAA014',                               'YAA148','YCG005T','YDB001','YTL109','YWK001');                SELECT 'SYSPACKAGE',CHAR(CREATOR,8),CHAR(NAME,20) AS PACKG_NAME,                                    CHAR(COLLID,15),BINDTIME                                    FROM SYSIBM.SYSPACKAGE                                                          WHERE CREATOR IN('A729204','A837065','D66179T','D70052T','D72913T',                              'D85267T','D92833T','D95834T','T105570','TQ00235',                              'TQ0061T','TQ0519','WKRDBAM5','X66179C','YAA014',                               'YAA148','YCG005T','YDB001','YTL109','YWK001');                SELECT 'SYSROUTINES',CHAR(SCHEMA,15),CHAR(OWNER,10),                                                 CHAR(NAME,20),ROUTINETYPE,CREATEDBY                        FROM SYSIBM.SYSROUTINES                                                         WHERE OWNER IN('A729204','A837065','D66179T','D70052T','D72913T',                                'D85267T','D92833T','D95834T','T105570','TQ00235',                              'TQ0061T','TQ0519','WKRDBAM5','X66179C','YAA014',                               'YAA148','YCG005T','YDB001','YTL109','YWK001');                SELECT * FROM SYSIBM.SYSPLANDEP                                                 WHERE BCREATOR IN('A729204','A837065','D66179T','D70052T','D72913T',                             'D85267T','D92833T','D95834T','T105570','TQ00235',                              'TQ0061T','TQ0519','WKRDBAM5','X66179C','YAA014',                               'YAA148','YCG005T','YDB001','YTL109','YWK001');                