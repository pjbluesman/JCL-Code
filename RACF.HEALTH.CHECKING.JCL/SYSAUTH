 SELECT CHAR(GRANTEE,8),                                                00887001            DATEGRANTED,SYSADMAUTH,SYSCTRLAUTH,                         00888000     SYSOPRAUTH FROM SYSIBM.SYSUSERAUTH WHERE                           00889000    (SYSOPRAUTH = 'Y' OR SYSCTRLAUTH = 'Y' OR SYSADMAUTH = 'Y' OR       00890000     SYSOPRAUTH = 'G' OR SYSCTRLAUTH = 'G' OR SYSADMAUTH = 'G')         00900000     ORDER BY 1,2;                                                      00910000