  SET CURRENT SQLID='LSEBDB2';                                                  -- To test delete                                                    --         --                                                                   --         --                                                                   --            LOCK TABLE DSN8910.PARTS IN EXCLUSIVE MODE;                                  --                                                                   --         -- Determine the number of rows the PARTS table starts with:         --         --                                                                   --         -- DELETE all rows :                                                 --         --                                                                   --            DELETE FROM DSN8910.PARTS ;                                                  --                                                                   --         -- Do a SELECT with an ORDER BY clause to verify that your data      --         -- sharing group can access the workfile data base:                  --         --                                                                   --         --                                                                   --         -- Count the number of rows again. It should have decreased to zero: --         --                                                                   --            SELECT COUNT(*) AS ITEM_COUNT FROM DSN8910.PARTS;                            