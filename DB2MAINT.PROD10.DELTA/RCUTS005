-- **************************************************************
-- *                                                            *
-- * TABLESPACE CREATE AND ALTER STATEMENTS                     *
-- *                                                            *
-- **************************************************************


-- DMRPRDB1.DMRPRTAD WILL BE CREATED VIA THESE NATIVE DB2 COMMANDS.

.AUTH  DT0072
    SET CURRENT SQLID = 'SYSPRO';

    CREATE TABLESPACE DMRPRTAD
           IN DMRPRDB1
                     USING STOGROUP DMRPRSG1
                                    PRIQTY 72000
                                    SECQTY 7200
                                    ERASE NO
                     FREEPAGE 0
                     PCTFREE 5
                     COMPRESS YES
           BUFFERPOOL BP3
           LOCKSIZE ANY
           CLOSE NO
           SEGSIZE 32
           LOCKMAX SYSTEM
           CCSID EBCDIC
    ;

.SYNC 15        'CREATE TABLESPACE DMRPRDB1.DMRPRTAD'
    SET CURRENT SQLID = USER;


