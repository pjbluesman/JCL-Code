-- "SYSIBM".SYSINDEXES_HIST WILL BE CREATED VIA THESE NATIVE DB2 COMMAND
-- S.


CREATE  TABLE "SYSIBM".SYSINDEXES_HIST
        ( "NAME" VARCHAR ( 128 ) NOT NULL
        , CREATOR VARCHAR ( 128 ) NOT NULL
        , TBNAME VARCHAR ( 128 ) NOT NULL
        , TBCREATOR VARCHAR ( 128 ) NOT NULL
        , CLUSTERING CHAR ( 1 ) NOT NULL
        , NLEAF INTEGER NOT NULL
          WITH DEFAULT
        - 1
        , NLEVELS SMALLINT NOT NULL
          WITH DEFAULT
        - 1
        , STATSTIME TIMESTAMP NOT NULL
        , FIRSTKEYCARDF FLOAT NOT NULL
          WITH DEFAULT
        - 1
        , FULLKEYCARDF FLOAT NOT NULL
          WITH DEFAULT
        - 1
        , CLUSTERRATIOF FLOAT NOT NULL
        , SPACEF FLOAT NOT NULL
          WITH DEFAULT
        - 1
        , IBMREQD CHAR ( 1 ) NOT NULL
          WITH DEFAULT
        'N'
        , AVGKEYLEN INTEGER NOT NULL
          WITH DEFAULT
        - 1
        , DATAREPEATFACTORF FLOAT NOT NULL
          WITH DEFAULT
        - 1
        , CONSTRAINT DSNIXH_CLSTRATIOF CHECK (
  (CLUSTERRATIOF >= 0 AND         CLUSTERRATIOF <= 1) OR         CLUSTER
RATIOF = -2 )
        , CONSTRAINT DSNIXH_FIRSTKCARDF CHECK (
  FIRSTKEYCARDF = -1 OR         FIRSTKEYCARDF >= 0 )
        , CONSTRAINT DSNIXH_FULLKCARDF CHECK (
  FULLKEYCARDF = -1 OR         FULLKEYCARDF >= 0 )
        , CONSTRAINT DSNIXH_NLEAF CHECK (
  NLEAF = -1 OR NLEAF >= 0 )
        , CONSTRAINT DSNIXH_NLEVELS CHECK (
  NLEVELS = -1 OR NLEVELS >= 0 )
        , CONSTRAINT DSNIXH_SPACEF CHECK (
  SPACEF = -1 OR SPACEF >= 0 )
        )
        IN DSNDB06.SYSHIST
   CCSID         UNICODE
   ;


