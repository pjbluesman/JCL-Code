-- **************************************************************


-- RDBI.FOREIGN_KEY_VIEW WILL BE CREATED VIA THESE NATIVE DB2 COMMANDS.

    SET CURRENT SQLID = 'RDBI';


CREATE  VIEW RDBI.FOREIGN_KEY_VIEW
        ( "TABLE_SCHEMA" , "TABLE_NAME" , "COLUMN_NAME" , "KEY_SEQ" ) AS
 SELECT  A."CREATOR" , A."TBNAME" , B."COLNAME" , B."COLSEQ"
   FROM "SYSIBM"."SYSRELS" A , "SYSIBM"."SYSFOREIGNKEYS" B
  WHERE A."RELNAME" = B."RELNAME"
;

COMMENT ON TABLE RDBI.FOREIGN_KEY_VIEW IS
        'Foreign key listing view for RDBI' ;
    SET CURRENT SQLID = USER;

    SET CURRENT SQLID = '@DBATFS';

    GRANT DELETE
         ,INSERT
         ,SELECT
         ,UPDATE
                    ON TABLE RDBI.FOREIGN_KEY_VIEW
                      TO      @DBATFD
                      WITH GRANT OPTION;

    SET CURRENT SQLID = '@DBATFS';

    GRANT SELECT
                    ON TABLE RDBI.FOREIGN_KEY_VIEW
                      TO      "PUBLIC";

    SET CURRENT SQLID = USER;

