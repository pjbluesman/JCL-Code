                      TO      @DBATFD
                      WITH GRANT OPTION;

    SET CURRENT SQLID = '@DBATFC';

    GRANT IMAGCOPY   ON DATABASE DSNDB06
                      TO      SWH3646;

    SET CURRENT SQLID = USER;

