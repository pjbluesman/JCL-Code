--SELECT * FROM SYSIBM.SYSCOPY
--   WHERE JOBNAME IN ('DBT401IC', 'DBT402IC')
--;
--FIM;
SELECT DISTINCT JOBNAME,ICTYPE
FROM SYSIBM.SYSCOPY
WHERE TSNAME IN(
SELECT TABLESPACE_NM FROM @DBA.OBSOLETE_DECOM_OBJECTS
   WHERE DECOM_CHNG_NBR='NPC00122584')   -- ROSA
-- WHERE DECOM_CHNG_NBR='NPC00122082')   -- RENE
-- WHERE DECOM_CHNG_NBR='NPC00123368')   -- JONAS
ORDER BY JOBNAME
WITH UR;
