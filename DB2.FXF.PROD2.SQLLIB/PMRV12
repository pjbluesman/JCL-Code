SELECT *
  FROM PRODCHG.PLAN_TABLE
 WHERE PROGNAME = 'PT15I002'
   AND QUERYNO IN (2322)
   AND BIND_TIME IN (
   '2019-04-13-22.41.46.539689',
   '2019-09-27-10.04.14.470519',
   '2019-09-21-23.08.16.731811')
 ORDER BY BIND_TIME,QUERYNO, QBLOCKNO,PLANNO;
