SELECT * FROM @DBA.REBIND_OBJ
 WHERE NAME = 'DBREVIEW'
 FETCH FIRST 1000 ROWS ONLY
;

SELECT NAME,COUNT(*) FROM @DBA.REBIND_OBJ
-- WHERE NAME = 'DBREVIEW'
 GROUP BY NAME
 HAVING COUNT(*) > 1
 ORDER BY 2 DESC
 FETCH FIRST 1000 ROWS ONLY
;
SELECT * FROM @DBA.REBIND_OBJ
 WHERE NAME = 'CBRIDBS'
 FETCH FIRST 1000 ROWS ONLY
;

SELECT        NAME,VERSION,COUNT(*)
 FROM @DBA.REBIND_OBJ
 WHERE NAME = 'CBRIDBS'
 GROUP BY        NAME,VERSION
 FETCH FIRST 1000 ROWS ONLY
;

SELECT SUM(QTY) FROM (
    SELECT NAME,COUNT(*) AS QTY FROM @DBA.REBIND_OBJ
    -- WHERE NAME = 'DBREVIEW'
     GROUP BY NAME) AS TEMP
;
SELECT SUM(QTY) FROM (
    SELECT NAME,COUNT(*) AS QTY FROM @DBA.REBIND_OBJ
    -- WHERE NAME = 'DBREVIEW'
     GROUP BY NAME
    HAVING COUNT(*) > 1 ) AS TEMP
;
