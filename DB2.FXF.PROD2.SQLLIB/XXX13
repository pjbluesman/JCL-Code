SELECT DISTINCT A.DBNAME,A.TSNAME,C.NTABLES,B.CREATOR,B.NAME
--   , A.JOBNAME,A.ICTYPE,B.CARDF
  FROM SYSIBM.SYSCOPY A
     , SYSIBM.SYSTABLES B
     , SYSIBM.SYSTABLESPACE C
  WHERE A.DBNAME = B.DBNAME
    AND A.TSNAME = B.TSNAME
    AND A.DBNAME = C.DBNAME
    AND A.TSNAME = C.NAME
    AND B.TYPE = 'T'
    AND B.NAME IN ('CS_ERRORS', 'CS_PAYABLE', 'CS_SYSPARMS')
;
SELECT DISTINCT A.JOBNAME,A.ICTYPE,A.DBNAME,A.TSNAME
  FROM SYSIBM.SYSCOPY A,
       SYSIBM.SYSTABLES B
  WHERE A.DBNAME = B.DBNAME
    AND A.TSNAME = B.TSNAME
    AND B.TYPE = 'T'
    AND B.NAME IN ('CS_ERRORS', 'CS_PAYABLE', 'CS_SYSPARMS')
