--
FIM;
SELECT A.CREATOR AS CREATOR,
A.TABLE_NM AS TABLE,
A.TABLESPACE_NM AS TABLESPACE,
A.DATABASE_NM AS DATABASE,
B.COMMENT_TXT AS DETAILS
,B.CREATE_DATE
FROM @DBA.OBSOLETE_DECOM_OBJECTS A, FFTA.IC_CONTROL B
WHERE A.TABLESPACE_NM = B.TSNAME_NM
AND   A.DATABASE_NM = B.DBNAME_NM
-- AND B.CREATE_DATE = DATE(CURRENT TIMESTAMP)
;
--
SELECT A.CREATOR,A.NAME, B.CREATOR,B.TABLE_NM
     , DECOM_CHNG_NBR
FROM SYSIBM.SYSTABLES A
   , @DBA.OBSOLETE_DECOM_OBJECTS  B
WHERE A.TYPE = 'T'
  AND A.CREATOR = B.CREATOR
  AND A.NAME    = B.TABLE_NM
  AND STRIP(A.DBNAME)||'.'||STRIP(A.TSNAME) IN (
'UCUCSTP.TCUCN',
'USHFSYS.TCSMI',
'USHFSYS.TCSMN',
'USHFSYS.TPASC',
'USHFSYS.TRWHB',
'USHFSYS.TRWHH',
'USH001.TFTPE',
'USH001.TFTPL',
'USH001.TFTPP',
'USH001.TFTPU',
'USH001.TFTPU',
'USH001.TPRYC',
'USH001.TPYCH',
'USH002.TSIAH')
;
FIM;
SELECT DISTINCT JOBNAME, DBNAME, TSNAME, ICTYPE, OTYPE, GROUP_MEMBER
FROM SYSIBM.SYSCOPY
WHERE TSNAME IN(
SELECT TABLESPACE_NM FROM @DBA.OBSOLETE_DECOM_OBJECTS
WHERE DECOM_CHNG_NBR='NPC00122584')
ORDER BY JOBNAME, DBNAME, TSNAME, ICTYPE
WITH UR;
----
FIM;
