SELECT *
  FROM FXFQ.IC_CONTROL C
  WHERE EXISTS (SELECT 1 FROM @DBA.OBSOLETE_DECOM_OBJECTS O
                 WHERE O.DATABASE_NM    = C.DBNAME_NM
                   AND O.TABLESPACE_NM  = C.TSNAME_NM
                   AND O.DECOM_CHNG_NBR='CHG0329968')
;
FIM;
SELECT DATABASE_NM,
       TABLESPACE_NM
  FROM @DBA.OBSOLETE_DECOM_OBJECTS
WHERE DECOM_CHNG_NBR='CHG0329968'
GROUP BY CREATOR,TABLE_NM
ORDER BY 3 DESC
;
FIM;
SELECT DATABASE_NM, TABLESPACE_NM,
COUNT (*)
FROM @DBA.OBSOLETE_DECOM_OBJECTS
WHERE  DECOM_CHNG_NBR =  'CHG0329968'
GROUP BY  DATABASE_NM, TABLESPACE_NM
ORDER BY 3 DESC
