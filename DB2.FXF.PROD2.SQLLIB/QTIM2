SELECT 'REBIND PACKAGE('||STRIP(COLLID)||'.'
       ||CASE WHEN NAME = UPPER(NAME)
              THEN STRIP(NAME)
              ELSE '*'
         END
       ||CASE WHEN VERSION <> ' '
              THEN  '.('||STRIP(VERSION)||'))'
              ELSE ')'
         END
  FROM JXS0036.REBIND_OBJ
 WHERE CATEGORY = 'COMMON'
   AND PLAN_PACKAGE = 'PACKAGE'
   AND PKG_TYPE = ' '
 ORDER BY COLLID, NAME, VERSION
 --FETCH FIRST 1000 ROWS ONLY
  WITH UR
;
COMMIT;
SELECT 'REBIND PACKAGE('||STRIP(COLLID)||'.'
       ||CASE WHEN NAME = UPPER(NAME)
              THEN STRIP(NAME)
              ELSE '*'
           END
       ||CASE WHEN VERSION <> ' '
                THEN  '.('||STRIP(VERSION)||'))'
                ELSE ')'
           END
  FROM JXS0036.REBIND_OBJ
 WHERE CATEGORY = 'JAVA'
   AND PLAN_PACKAGE = 'PACKAGE'
   AND PKG_TYPE = ' '
 ORDER BY COLLID, NAME, VERSION
 --FETCH FIRST 1000 ROWS ONLY
  WITH UR
;
COMMIT;
SELECT CHAR('REBIND PLAN('||STRIP(NAME)||')  ',80)
     , NAME, 01
  FROM JXS0036.REBIND_OBJ
 WHERE CATEGORY = 'VENDOR'
   AND PLAN_PACKAGE = 'PLAN'
 ORDER BY 2, 3
 --FETCH FIRST 1000 ROWS ONLY
  WITH UR
;
COMMIT;
SELECT 'REBIND PACKAGE('||STRIP(COLLID)||'.'
       ||CASE WHEN NAME = UPPER(NAME)
              THEN STRIP(NAME)
              ELSE '*'
         END
       ||CASE WHEN VERSION <> ' '
              THEN  '.('||STRIP(VERSION)||'))'
              ELSE ')'
         END
  FROM JXS0036.REBIND_OBJ
 WHERE CATEGORY = 'VENDOR'
   AND PLAN_PACKAGE = 'PACKAGE'
   AND PKG_TYPE = ' '
   AND NAME < 'E'
 ORDER BY COLLID, NAME, VERSION
 --FETCH FIRST 1000 ROWS ONLY
  WITH UR
;
COMMIT;
SELECT 'REBIND PACKAGE('||STRIP(COLLID)||'.'
       ||CASE WHEN NAME = UPPER(NAME)
              THEN STRIP(NAME)
              ELSE '*'
         END
       ||CASE WHEN VERSION <> ' '
              THEN  '.('||STRIP(VERSION)||'))'
              ELSE ')'
         END
  FROM JXS0036.REBIND_OBJ
 WHERE CATEGORY = 'VENDOR'
   AND PLAN_PACKAGE = 'PACKAGE'
   AND PKG_TYPE = ' '
   AND (NAME BETWEEN 'E' AND 'GDAT')
 ORDER BY COLLID, NAME, VERSION
 --FETCH FIRST 1000 ROWS ONLY
  WITH UR
;
COMMIT;
SELECT 'REBIND PACKAGE('||STRIP(COLLID)||'.'
       ||CASE WHEN NAME = UPPER(NAME)
              THEN STRIP(NAME)
              ELSE '*'
         END
       ||CASE WHEN VERSION <> ' '
              THEN  '.('||STRIP(VERSION)||'))'
              ELSE ')'
         END
  FROM JXS0036.REBIND_OBJ
 WHERE CATEGORY = 'VENDOR'
   AND PLAN_PACKAGE = 'PACKAGE'
   AND PKG_TYPE = ' '
   AND (NAME BETWEEN 'GDAT' AND 'H')
 ORDER BY COLLID, NAME, VERSION
 --FETCH FIRST 1000 ROWS ONLY
  WITH UR
;
COMMIT;
SELECT 'REBIND PACKAGE('||STRIP(COLLID)||'.'
       ||CASE WHEN NAME = UPPER(NAME)
              THEN STRIP(NAME)
              ELSE '*'
         END
       ||CASE WHEN VERSION <> ' '
              THEN  '.('||STRIP(VERSION)||'))'
              ELSE ')'
         END
  FROM JXS0036.REBIND_OBJ
 WHERE CATEGORY = 'VENDOR'
   AND PLAN_PACKAGE = 'PACKAGE'
   AND PKG_TYPE = ' '
   AND (NAME BETWEEN 'H' AND 'RMC')
 ORDER BY COLLID, NAME, VERSION
 --FETCH FIRST 1000 ROWS ONLY
  WITH UR
;
COMMIT;
SELECT 'REBIND PACKAGE('||STRIP(COLLID)||'.'
       ||CASE WHEN NAME = UPPER(NAME)
              THEN STRIP(NAME)
              ELSE '*'
         END
       ||CASE WHEN VERSION <> ' '
              THEN  '.('||STRIP(VERSION)||'))'
              ELSE ')'
         END
  FROM JXS0036.REBIND_OBJ
 WHERE CATEGORY = 'VENDOR'
   AND PLAN_PACKAGE = 'PACKAGE'
   AND PKG_TYPE = ' '
   AND (NAME BETWEEN 'RMC' AND 'T')
 ORDER BY COLLID, NAME, VERSION
 --FETCH FIRST 1000 ROWS ONLY
  WITH UR
;
COMMIT;
SELECT 'REBIND PACKAGE('||STRIP(COLLID)||'.'
       ||CASE WHEN NAME = UPPER(NAME)
              THEN STRIP(NAME)
              ELSE '*'
         END
       ||CASE WHEN VERSION <> ' '
              THEN  '.('||STRIP(VERSION)||'))'
              ELSE ')'
         END
  FROM JXS0036.REBIND_OBJ
 WHERE CATEGORY = 'VENDOR'
   AND PLAN_PACKAGE = 'PACKAGE'
   AND PKG_TYPE = ' '
   AND NAME > 'T'
 ORDER BY COLLID, NAME, VERSION
 --FETCH FIRST 1000 ROWS ONLY
  WITH UR
;
COMMIT;
SELECT CHAR('REBIND PLAN('||STRIP(NAME)||') -',80)
     , NAME, 01
  FROM JXS0036.REBIND_OBJ
 WHERE CATEGORY = 'LINEHAUL'
   AND PLAN_PACKAGE = 'PLAN'
 UNION
SELECT CHAR('       EXPLAIN(YES)',80)
     , NAME, 02
  FROM JXS0036.REBIND_OBJ
 WHERE CATEGORY = 'LINEHAUL'
   AND PLAN_PACKAGE = 'PLAN'
 ORDER BY 2, 3
 --FETCH FIRST 1000 ROWS ONLY
  WITH UR
;
COMMIT;
