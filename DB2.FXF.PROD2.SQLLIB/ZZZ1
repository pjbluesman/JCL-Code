SELECT NAME,LENGTH,COLTYPE,COLNO
  FROM SYSIBM.SYSCOLUMNS
 WHERE TBCREATOR = 'TFMSV15P'
   AND TBNAME    = 'TFMTB027'
   AND NAME IN
('TF_CHGID', 'TF_CHGDT', 'TF_CHGTM', 'TF_CMPNY', 'TF_CTLTYP',
'TF_RECID' , 'TF_KEY', 'TF_FLD1V', 'TF_USERID', 'TF_RDESC')
ORDER BY COLNO
WITH UR;
;
FIM;
SELECT COUNT(*) FROM PROD.TFMTB027
 WHERE TF_RECID = 'VFS-MWT'
AND TF_KEY > ''
