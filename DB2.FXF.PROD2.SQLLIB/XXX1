SELECT
       'DROP TABLE '||STRIP(CREATOR)||'.'||STRIP(NAME)||'; COMMIT;'||
 '   --'||STRIP(DBNAME)||'.'||STRIP(TSNAME)
  FROM SYSIBM.SYSTABLES
 WHERE STRIP(DBNAME)||'.'||STRIP(TSNAME) IN (
'IPRPS12.THPPY' ,
'IPRPS26.TPSFA' ,
'IPRPS26.TPSFL' ,
'IPRPS26.TPSGA' ,
'UPRPS01.THW16' ,
'UPRPS03.TAD02' ,
'UPRPS04.THF02' ,
'UPRPS04.THF07' ,
'UPRPS04.THF13' ,
'UPRPS04.THF16' ,
'UPRPS12.THPPY' ,
'UPRPS26.TPSFA' ,
'UPRPS26.TPSFL' ,
'UPRPS26.TPSGA')
 AND TYPE = 'T'
