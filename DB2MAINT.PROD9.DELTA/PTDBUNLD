.SYSTEM UNLOADS
.CONNECT DB2N

.ALLOC FI(PTIUNLD)                                                     +
       DA('DB2N.PTI.ACCUM$ST.RATEGY.DATA')                             +
       BLKSIZE(6336) LRECL(72)                                         +
       TRACKS                                                          +
       SPACE(14,11)                                                    +
       UNIT(SYSDA)                                                     +
       NEW CATALOG
.CALL UNLOAD
.DATA
  FILE(PTIUNLD)
  SELECT STRATEGY_CREATOR, STRATEGY_NAME, SEQUENCE_FIELD,
  INTERNAL_TYPE, "DATA"
  FROM PTI.ACCUM_STRATEGY
  FOR FETCH ONLY
  ;
  LIMIT(ALL)
.ENDDATA
.FREE FI(PTIUNLD)

.SYNC 5         'UNLOAD TABLE PTI.ACCUM_STRATEGY'


.ALLOC FI(PTIUNLD)                                                     +
