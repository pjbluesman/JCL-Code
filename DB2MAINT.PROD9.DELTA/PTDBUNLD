  PRFM_ARCH_WTIME, PRFM_ARCHWCNT, PRFM_DRAIN_WTIME,
  PRFM_DRAINWCNT, PRFM_CLAIM_WTIME, PRFM_CLAIMWCNT,
  PRFM_ARCRD_WTIME, PRFM_ARCRDWCNT, PRFM_PLTCH_WTIME,
  PRFM_PLTCHWCNT, PRFM_SND_MSGTME, PRFM_SND_MSGCNT,
  PRFM_GLB_LOCKTME, PRFM_GLB_LOCKCNT, PRFM_IOPNOENCLAV,
  PRFM_IOPSINGCORD, PRFM_IOPSINGRSRR, PRFM_IOPDATAGRPS,
  PRFM_IOPSKIPBFRP, PRFM_NUM_REOPTS, PRFM_XES_LOCKREQ,
  PRFM_XES_CHNGREQ, PRFM_XES_UNLKREQ, PRFM_SUSPND_IRLM,
  PRFM_SUSPND_XES, PRFM_SUSPNDFALSE, PRFM_GLBLOCKFAIL,
  PRFM_FND_CACHE, PRFM_NOTFND_CACHE, PRFM_IMPL_PREPARE,
  PRFM_SKIP_PREPARE, PRFM_CONNID, PRFM_CORRID, PRFM_AUTHID,
  PRFM_OPID, PRFM_STMTNOI, PRFM_SECTNOI, PRFM_VERSION
  FROM PTI.PTAN_PRFM_0201
  ORDER BY PRFM_PGI , PRFM_STMT_ID , PRFM_TYPE DESC ,
