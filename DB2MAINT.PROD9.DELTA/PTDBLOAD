   MOD CATALOG UNIT(SYSDA)                                             +
   SPACE(5,7) BLK(4096) RLSE
.ALLOC FI(SYSMAP)                                                      +
   DA('DB2N.PTI.ALOGFILE.DATA.M')                                      +
   MOD CATALOG UNIT(SYSDA)                                             +
   SPACE(1,2) BLK(4096) RLSE
.ALLOC FI(PTIUT1) UNIT(SYSDA)                                          +
