.SYNC 1500      'LOAD TABLE PTI.RATB_STATS_0206'

.CALL UTIL LOAD PARM(DB2N)
.ALLOC FI(PTILOAD)                                                     +
   DA('DB2N.PTI.RATS$STA.TS$0206.DATA')                                +
   SHR
.ALLOC FI(SYSDISC)                                                     +
   DA('DB2N.PTI.RATS$STA.TS$0206.DATA.D')                              +
   MOD CATALOG UNIT(SYSDA)                                             +
   SPACE(1,2) TRACKS RLSE
.ALLOC FI(SYSERR)                                                      +
   DA('DB2N.PTI.RATS$STA.TS$0206.DATA.E')                              +
   MOD CATALOG UNIT(SYSDA)                                             +
   SPACE(8,10) BLK(4096) RLSE
.ALLOC FI(SYSMAP)                                                      +
   DA('DB2N.PTI.RATS$STA.TS$0206.DATA.M')                              +
   MOD CATALOG UNIT(SYSDA)                                             +
   SPACE(2,2) BLK(4096) RLSE
.ALLOC FI(PTIUT1) UNIT(SYSDA)                                          +
   DA('DB2N.PTI.RATS$STA.TS$0206.DATA.U')                              +
   SPACE(1,1) CYL                                                      +
   MOD CATALOG
.ALLOC FI(PTIOUT) UNIT(SYSDA)                                          +
   DA('DB2N.PTI.RATS$STA.TS$0206.DATA.O')                              +
   SPACE(1,1) CYL                                                      +
   MOD CATALOG
.DATA
   LOAD DATA INDDN PTILOAD RESUME NO  REPLACE  LOG YES
    ENFORCE CONSTRAINTS DISCARDDN SYSDISC
    ERRDDN   SYSERR
    MAPDDN   SYSMAP
    WORKDDN  (PTIUT1,PTIOUT)
    SORTDEVT SYSDA
    CONTINUEIF (72) = X'FF'
    INTO TABLE PTI.RATS_STATS_0206
