.CONTROL SN(PTDB,PLATLOAD)

.SYSTEM SQLDDL

.SYNC 760       'UTILITIES SECTION'

-- **************************************************************
-- *                                                            *
-- * UTILITY STATEMENTS                                         *
-- *                                                            *
-- **************************************************************


.CALL UTIL LOAD PARM(DB2N)
.ALLOC FI(PTILOAD)                                                     +
   DA('DB2N.PTI.ACCUM$ST.RATEGY.DATA')                                 +
   SHR
.ALLOC FI(SYSDISC)                                                     +
   DA('DB2N.PTI.ACCUM$ST.RATEGY.DATA.D')                               +
   MOD CATALOG UNIT(SYSDA)                                             +
   SPACE(4,7) TRACKS RLSE
.ALLOC FI(SYSERR)                                                      +
   DA('DB2N.PTI.ACCUM$ST.RATEGY.DATA.E')                               +
   MOD CATALOG UNIT(SYSDA)                                             +
   SPACE(18,24) BLK(4096) RLSE
.ALLOC FI(SYSMAP)                                                      +
   DA('DB2N.PTI.ACCUM$ST.RATEGY.DATA.M')                               +
   MOD CATALOG UNIT(SYSDA)                                             +
   SPACE(4,5) BLK(4096) RLSE
.ALLOC FI(PTIUT1) UNIT(SYSDA)                                          +
   DA('DB2N.PTI.ACCUM$ST.RATEGY.DATA.U')                               +
   SPACE(1,1) CYL                                                      +
   MOD CATALOG
.ALLOC FI(PTIOUT) UNIT(SYSDA)                                          +
   DA('DB2N.PTI.ACCUM$ST.RATEGY.DATA.O')                               +
   SPACE(1,1) CYL                                                      +
   MOD CATALOG
.DATA
   LOAD DATA INDDN PTILOAD RESUME NO  REPLACE  LOG YES
    ENFORCE CONSTRAINTS DISCARDDN SYSDISC
    ERRDDN   SYSERR
    MAPDDN   SYSMAP
    WORKDDN  (PTIUT1,PTIOUT)
    SORTDEVT SYSDA
    CONTINUEIF (72) = X'FF'
    INTO TABLE PTI.ACCUM_STRATEGY
    (STRATEGY_CREATOR     POSITION(     1) CHAR(     8)
    ,STRATEGY_NAME        POSITION(     *) CHAR(     8)
    ,SEQUENCE_FIELD       POSITION(     *) SMALLINT
    ,INTERNAL_TYPE        POSITION(     *) SMALLINT
    ,"DATA"               POSITION(     *) VARCHAR
    )
.ENDDATA
.FREE FI(PTILOAD)
.FREE FI(SYSDISC)
.FREE FI(SYSERR)
.FREE FI(SYSMAP)
.FREE FI(PTIUT1)
.FREE FI(PTIOUT)

.SYNC 765       'LOAD TABLE PTI.ACCUM_STRATEGY'

.CALL UTIL LOAD PARM(DB2N)
.ALLOC FI(PTILOAD)                                                     +
   DA('DB2N.PTI.ALOGFILE.DATA')                                        +
   SHR
.ALLOC FI(SYSDISC)                                                     +
