   SPACE(1,1) CYL                                                      +
   MOD CATALOG
.DATA
   LOAD DATA INDDN PTILOAD
    RESUME NO  REPLACE LOG YES
    ENFORCE CONSTRAINTS DISCARDDN SYSDISC
    ERRDDN   SYSERR
    MAPDDN   SYSMAP
    WORKDDN  (PTIUT1,PTIOUT)
    SORTDEVT SYSDA
    CONTINUEIF (72) = X'FF'
    INTO TABLE PTI.PDT_HST01_DYNAMREQ
    (SSID                 POSITION(     1) CHAR(     4)
    ,INTERVAL_START       POSITION(     *) TIMESTAMP EXTERNAL(    26)
    ,INTERVAL_END         POSITION(     *) TIMESTAMP EXTERNAL(    26)
