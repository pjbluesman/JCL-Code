    ,STRATEGY_NAME        POSITION(     *) CHAR(     8)
    ,ACCUM_KEY            POSITION(     *) CHAR(     8)
    ,STATUS               POSITION(     *) CHAR(     4)
    ,DSNAME               POSITION(     *) CHAR(    44)
    ,UNIT                 POSITION(     *) CHAR(     1)
    ,DEVTYPE              POSITION(     *) CHAR(     8)
    ,FILESEQ              POSITION(     *) SMALLINT
    ,"TYPE"               POSITION(     *) CHAR(     2)
    ,DATE                 POSITION(     *) CHAR(     6)
    ,TIME                 POSITION(     *) CHAR(     6)
    ,DSVOLSER             POSITION(     *) VARCHAR
    )
.ENDDATA
.FREE FI(PTILOAD)
.FREE FI(SYSDISC)
.FREE FI(SYSERR)
.FREE FI(SYSMAP)
.FREE FI(PTIUT1)
.FREE FI(PTIOUT)

