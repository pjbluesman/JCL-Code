.SYNC 775       'LOAD TABLE PTI.ALOGRANGE'


.CALL UTIL LOAD PARM(DB2N)
.ALLOC FI(PTILOAD)                                                     +
   DA('DB2N.PTI.PDT$HST0.#1$DYNAM.REQ.DATA')                           +
   SHR
.ALLOC FI(SYSDISC)                                                     +
   DA('DB2N.PTI.PDT$HST0.#1$DYNAM.REQ.DATA.D')                         +
   MOD CATALOG UNIT(SYSDA)                                             +
   SPACE(4,7) TRACKS RLSE
.ALLOC FI(SYSERR)                                                      +
