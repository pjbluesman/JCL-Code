    ,OPID                 POSITION(     *) CHAR(     8)
    ,SQL_CALL             POSITION(     *) CHAR(    16)
    ,SQL_CALLS            POSITION(     *) INTEGER
    ,SECT#                POSITION(     *) SMALLINT
    ,STMT#                POSITION(     *) SMALLINT
    ,"PROGRAM"            POSITION(     *) CHAR(     8)
    ,PLANNAME             POSITION(     *) CHAR(     8)
    ,LOCATION             POSITION(     *) CHAR(    16)
    ,CONNID               POSITION(     *) CHAR(     8)
    ,CORRID               POSITION(     *) CHAR(    12)
    ,AUTHID               POSITION(     *) CHAR(     8)
