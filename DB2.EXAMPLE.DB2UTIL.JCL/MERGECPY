//JS1      EXEC DB2BATCH,ENV=SBOX,SSID=DSNS,UID=DB2MERGECPY
//SYSUT1   DD  UNIT=SYSDA,SPACE=(CYL,(1000,1000))
//SYSIN    DD  DISP=SHR,DSN=AFDB.PROD.PARMLIB(OPTNSKIP)
//         DD  DISP=SHR,DSN=AFDB.&ENV..TEMPLATE(LPICTAPE)
//         DD  *
LISTDEF LDEFTS
        INCLUDE TABLESPACE ITPADBA.TDSNS

MERGECOPY LIST LDEFTS
          NEWCOPY YES
          COPYDDN (LPCOPY)
