SET CURRENT SQLID='PRODDBA';

EXPLAIN ALL SET QUERYNO=5270036 FOR
SELECT  A.PO_NUMBER , A.PRO_NUMBER , A.SUFFIX_NUMBER
   FROM PROD.SHP_PO_NBR_XREF A , PROD.SHP_SHIP_TSP_XREF B
  WHERE A.PO_NUMBER LIKE ?
    AND B.SHIP_TIMESTAMP BETWEEN ?
    AND ?
    AND B.PRO_NUMBER = A.PRO_NUMBER
    AND B.SUFFIX_NUMBER = A.SUFFIX_NUMBER
 OPTIMIZE FOR 2 ROWS
;

SELECT * FROM PRODDBA.PLAN_TABLE
 WHERE QUERYNO=5270036
 ORDER BY QUERYNO,QBLOCKNO,PLANNO;
DELETE  FROM PRODDBA.PLAN_TABLE
 WHERE QUERYNO=5270036;
COMMIT;
