
  SELECT CASE WHEN COUNT(*) > 1
           THEN CHAR('CATMAINT UPDATE UNLDDN PI34964',80) --WAS PI18738
           ELSE CHAR(' ',80)
         END
 -- R.RELNAME
     FROM SYSIBM.SYSRELS AS R
    WHERE R.CREATOR='SYSIBM'
      AND R.TBNAME='SYSUTILX'
      AND R.RELNAME <> 'DSN1T'||X'40'||'1T';
