    SELECT PURCHASED_MSTR01 . "VNDR_NBR", PURCHASED_MSTR01 .
           "MSTR_BOL_NBR", PURCHASED_MSTR01 . "BOL_NBR",
           PURCHASED_MSTR01 . "SEQ_NBR", PURCHASED_MSTR01 .
           "RULE_11_FLG", PURCHASED_MSTR01 . "UID_NBR", PURCHASED_MSTR01
           . "CRTE_TMSTP", PURCHASED_LOAD02 . "BILL_OF_LADING_NBR",
           PURCHASED_LOAD02 . "STATUS_CODE", PURCHASED_LOAD02 .
           "BROKER_TYPE_CD", PURCHASED_LOAD02 . "FK_VENDOR_NBR",
           PURCHASED_TRLR03 . "TRAILER_SEQUENCE", PURCHASED_TRLR03 .
           "ORIG_CUST_CENTER", PURCHASED_TRLR03 . "DEST_CUST_CENTER",
           PURCHASED_TRLR03 . "FK_VENDOR_NBR", PURCHASED_TRLR03 .
           "FK_BOL_NBR"
    FROM "PURCHASED_MSTR" PURCHASED_MSTR01, "PURCHASED_LOAD"
         PURCHASED_LOAD02, "PURCHASED_TRLR" PURCHASED_TRLR03
    WHERE PURCHASED_MSTR01 . "MSTR_BOL_NBR" = : H
          AND PURCHASED_LOAD02 . "BILL_OF_LADING_NBR" = PURCHASED_MSTR01
          . "BOL_NBR"
          AND PURCHASED_TRLR03 . "FK_VENDOR_NBR" = PURCHASED_LOAD02 .
          "FK_VENDOR_NBR"
          AND PURCHASED_TRLR03 . "FK_BOL_NBR" = PURCHASED_LOAD02 .
          "BILL_OF_LADING_NBR"
          AND PURCHASED_TRLR03 . "TRAILER_SEQUENCE" = 1
    ORDER BY 4 DESC
























































































































































































































































































































































































