SELECT CASE WHEN COUNT(*) > 1
                 THEN CHAR('CATMAINT UPDATE UNLDDN PI08928',80)
                 ELSE CHAR(' ',80)
       END
    FROM SYSIBM.SYSCOLUMNS
    WHERE TBCREATOR = 'SYSIBM' AND TBNAME IN
    ('SYSUTIL','DBDR','SCTR','SPTR','SYSLGRNX','SYSUTILX') AND
    LENGTH(LABEL) ^= 0;
