--SELECT STRIP(TBOWNER)||'.'||STRIP(TBNAME)
--  FROM SYSIBM.SYSAUXRELS
--UNION ALL
--SELECT STRIP(AUXTBOWNER)||'.'||STRIP(AUXTBNAME)
--  FROM SYSIBM.SYSAUXRELS
--;
--FIM;
SELECT DISTINCT
'INCLUDE TABLESPACE '||STRIP(S.DBNAME)||'.'||STRIP(S.NAME)
--T.CREATOR,T.NAME,S.TYPE,P.RBA_FORMAT,T.TYPE
FROM SYSIBM.SYSTABLESPACE S
   , SYSIBM.SYSTABLEPART  P
   , SYSIBM.SYSTABLES     T
WHERE S.NACTIVE > 0
  AND INT(LOG10(S.NACTIVE)) <= 5
  AND S.DBNAME = P.DBNAME
  AND S.NAME   = P.TSNAME
  AND P.RBA_FORMAT <> 'E'
  AND T.DBNAME = S.DBNAME
  AND T.TSNAME = S.NAME
  AND T.TYPE = 'T'
  AND NOT EXISTS (SELECT '1' FROM SYSIBM.SYSAUXRELS X
                  WHERE  T.NAME   = X.TBNAME
                    AND  T.CREATOR= X.TBOWNER)
--AND NOT EXISTS (SELECT '1' FROM SYSIBM.SYSAUXRELS X
--                WHERE  T.NAME   = X.AUXTBNAME
--                  AND  T.CREATOR= X.AUXTBOWNER)
--AND S.TYPE   = 'O'
--AND T.CREATOR = 'SYSIBM'
--AND T.NAME    = 'XSROBJECTS'
--AND S.DBNAME = 'RSBIDB' AND S.NAME IN ('RSBILOB', 'RSBILOB2'
--                                      ,'RSBISPC')
;
FIM;
  AND STRIP(T.CREATOR)||'.'||STRIP(T.NAME) IN (
