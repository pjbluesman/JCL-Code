SELECT CHAR(000.1),CHAR(1000.),CHAR(1.1),
       VARCHAR('00.10'),VARCHAR('1')
  FROM SYSIBM.SYSDUMMY1;

SET CURRENT PATH = SYSCOMPAT_V9;
SELECT CHAR(000.1),CHAR(1000.),CHAR(1.1),
       VARCHAR('00.10'),VARCHAR('1')
  FROM SYSIBM.SYSDUMMY1;

SET CURRENT PATH = SYSCURRENT,SYSIBM;
SELECT CHAR(000.1),CHAR(1000.),CHAR(1.1),
       VARCHAR('00.10'),VARCHAR('1')
  FROM SYSIBM.SYSDUMMY1;
