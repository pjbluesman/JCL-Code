SELECT STRIP(GETVARIABLE('SYSIBM.SSID')) AS SSID,
       SCREATOR, STNAME, GRANTEE, SELECTAUTH,
       INSERTAUTH, UPDATEAUTH,
       DELETEAUTH
FROM SYSIBM.SYSTABAUTH
WHERE STNAME IN ('FWT0095D', 'FWT0125D', 'OBT00X1D',
                 'OBT0003D', 'OBT0004D', 'OBT0005D',
                 'OBT0006D', 'OBT0007D', 'OBT0008D',
                 'OBT0009D')
 AND GRANTEETYPE <> 'P'
WITH UR;

SELECT STRIP(GETVARIABLE('SYSIBM.SSID')) AS SSID,
       GRANTOR, GRANTEE, DATEGRANTED, TIMEGRANTED,
       SYSADMAUTH, SYSOPRAUTH
  FROM SYSIBM.SYSUSERAUTH
 WHERE SYSADMAUTH = 'Y'
WITH UR;

SELECT STRIP(GETVARIABLE('SYSIBM.SSID')) AS SSID,
       GRANTOR, GRANTEE, DATEGRANTED, TIMEGRANTED,
       SYSADMAUTH, SYSOPRAUTH
  FROM SYSIBM.SYSUSERAUTH
 WHERE SYSOPRAUTH = 'Y'
WITH UR;
