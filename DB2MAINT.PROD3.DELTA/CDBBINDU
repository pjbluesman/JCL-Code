//UXD001M JOB (,102),'G KELLY',CLASS=S,MSGCLASS=X                       JOB04466
//*********************************************************************
//*          CONFIGURE CDB/AUTO-UTILITIES V740 FOR DB2=DB2U           *
//*********************************************************************
//SUTCFG1 EXEC PGM=CDBISTRT,
// PARM=('DB2U','0','Delta Technology'),REGION=0M
//STEPLIB   DD DISP=SHR,DSN=SSXDT.CDB.PTF19.LOADLIB
//SYSPRINT  DD SYSOUT=*
//SYSERROR  DD SYSOUT=*
//CNTL      DD DISP=SHR,DSN=SSXDT.CDB.PTF19.CNTL
//LOAD      DD DISP=SHR,DSN=SSXDT.CDB.PTF19.LOADLIB
//SYSPDS    DD DISP=SHR,DSN=SSXDT.CDB.PTF19.SYSPDS
//LOGPDS    DD DISP=SHR,DSN=SSXDT.CDB.PTF19.LOGPDS
//*-------------------------------------------------------------------*
//*               CDB/AUTO-UTILITIES GLOBAL PARAMETERS                *
//*-------------------------------------------------------------------*
//UTLPARMS  DD *
+COPYPENDING(YES)
+DB2DOWN(ABEND)
+DELETEWORK(YES)
+DISP(SHR)
+DSCRL(000100,100)
+DSNCAT(DSNCAT)
+EDPROCSUP(NO)
+FASTSWITCH(YES)
+HRECALL(NO)
+LISTSYSIN(YES)
+MAXSORTS(4)
+ONERROR(ABEND)
+PARMLIST(YES)
+PLAN(CDBPLAN)
+POST(INDIVIDUALLY)
+QAFTER(NO)
+QBEFORE(NO)
+QRETRY(002000,10)
+RACFID()
+REXXTIME(10)
+RERUN(0)
+RUN(EXECUTE)
+SYNC(ALLATONCE)
+VOLSCHED(NO)
+WORKUNIT(SYSDA,0)
+WORKPREFIX(SSXDT.CDB.WORK)
//*-------------------------------------------------------------------*
//*         CDB/AUTO-REORG TABLE SPACE AND SYSTEM PARAMETERS          *
//*-------------------------------------------------------------------*
//RRGPARMS  DD *
+AFTERACC(RW)
+BEFOREACC(RO)
+DOFIRST(NONE)
+ALLO(USED)
+FREEPAGE(-1)
+MAXTASKS(10)
+MARKFULL(NO)
+MODE(24X7)
+PCTFREE(-1)
+SPACEONLY(NO)
+INPLACEBUF(0)
+INITWINDOW(0)
+PAGEUSE(50)
+PARTRENAME(YES)
+RENAME(YES)
+REORGRETRY(001000,100)
+RUNSTATS(ALL)
+TRACE(63)
+VOLUMES()
+PASSWORD(4LQQ7ELDJX)
//*-------------------------------------------------------------------*
//*                  CDB/AUTO-REORG INDEX PARAMETERS                  *
//*-------------------------------------------------------------------*
//RRGPARMX  DD *
+ALLO(USED)
+FREEPAGE(-1)
+MODE(24X7)
+PCTFREE(-1)
+TREEFREE(-1)
+TREEPAGE(-1)
+VOLUMES()
//*-------------------------------------------------------------------*
//*                     CDB/AUTO-COPY PARAMETERS                      *
//*-------------------------------------------------------------------*
//CPYPARMS  DD *
+AFTERACC(RW)
+BEFOREACC(RO)
+COPIES(1)
+FULL_FORCE(NO)
+FULLIFDAYS(0)
+MAXTASKS(25)
+CHKRBA(N)
+COPYINDEX(N)
+NOCHANGE(KEEP)
+QDEPTH(100)
+RESETMOD(NO)
+TEST(NO)
+TRACE(63)
//*********************************************************************
//*            BIND PACKAGES AND PLAN CDBPLAN ON DB2=DB2U             *
//*********************************************************************
//SUTBND1 EXEC PGM=CDBBINDR,DYNAMNBR=20,COND=(0,LT,SUTCFG1)
//STEPLIB   DD DISP=SHR,DSN=SSXDT.CDB.PTF19.LOADLIB
//          DD DISP=SHR,DSN=SDB2U.SDSNLOAD
//DBRMLIB   DD DISP=SHR,DSN=SSXDT.CDB.PTF19.CNTL
//SYSTSPRT  DD SYSOUT=*
//SYSPRINT  DD SYSOUT=*
//SYSTSIN   DD UNIT=SYSDA,SPACE=(TRK,(1,1))
//CDBIN     DD *
 DSN SYSTEM(DB2U)
*
