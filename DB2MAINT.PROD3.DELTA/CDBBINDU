//*********************************************************************
//*        GRANT EXECUTE ON PLAN CDBPLAN TO PUBLIC ON DB2=DB2U        *
//*********************************************************************
//SUTGR1  EXEC PGM=IKJEFT01,DYNAMNBR=20,
//             COND=((0,LT,SUTCFG1),(0,LT,SUTBND1))
//STEPLIB   DD DISP=SHR,DSN=SDB2U.SDSNLOAD
//          DD DISP=SHR,DSN=SSXDT.CDB.PTF19.LOADLIB
//SYSTSPRT  DD SYSOUT=*
//SYSPRINT  DD SYSOUT=*
//SYSIN     DD *
 GRANT EXECUTE ON PLAN CDBPLAN TO PUBLIC;
//SYSTSIN   DD *
 DSN SYSTEM(DB2U)
*
 RUN PROGRAM(CDBSQLXI) -
 PLAN(CDBPLAN)
*
