   INTO NEW_TKT_DOC_NB, NEW_TKT_DOC_ISS_DT, NEW_TKT_DOC_SQ_NB,
   EXCGD_TKT_DOC_NB, EXCGD_TKDC_ISS_DT, EXCGD_TKDC_SQ_NB,
