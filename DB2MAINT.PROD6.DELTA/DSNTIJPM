//DB6RTJPM JOB ,'DB2 SYSTEM',CLASS=S,MSGCLASS=X,TIME=1440,
//         NOTIFY=&SYSUID,REGION=0M
/*JOBPARM SYSAFF=MVS4
//*********************************************************************
//* This member has been edited by the DB2BASE edit macro.            *
//*********************************************************************
//*********************************************************************
//*  NAME = DSNTIJPM
//*
//*  DESCRIPTIVE NAME = DB2 INSTALL JOB
//*
//*    Licensed Materials - Property of IBM
//*    5635-DB2
//*    (C) COPYRIGHT 1982, 2006 IBM Corp.  All Rights Reserved.
//*
//*    STATUS = Version 9
//*
//*  FUNCTION = RUN THIS JOB ON DB2 VERSION 8 PRIOR
//*             TO MIGRATION TO DB2 VERSION 9.  IT QUERIES THE DB2
//*             CATALOG TO IDENTIFY CONDITIONS THAT WILL NEED TO BE
//*             REMEDIED BEFORE YOU ATTEMPT TO MIGRATE TO DB2 VERSION
//*             9 OR THAT NEED TO BE ATTENDED TO AFTER YOU HAVE
//*             MIGRATED TO DB2 VERSION 9.
//*
//*             THIS JOB GENERATES REPORTS THAT IDENTIFY CONDITIONS
//*             ON DB2 VERSION 8 THAT NEED TO BE REMEDIED BEFORE YOU
//*             COMMENCE MIGRATION TO VERSION 9, OR THAT NEED TO BE
//*             ATTENDED TO AFTER YOU HAVE MIGRATED TO DB2 VERSION 9.
//*
//*  PSEUDOCODE =
//*    DELETE   STEP     REMOVE DATA SETS CREATED BY PREVIOUS RUNS
//*    UNLOAD   STEP     CALL DSNTIAUL TO PROCESS SQL STATEMENTS
//*    REPORTxx STEP     GENERATE REPORTS
//*
//*     NOTES = THE JCL SHOULD USE THE DSNTIAUL MODULE AND PLAN PRE-
//*             PARED FOR THE DB2 ENVIRONMENT YOU ARE MIGRATING FROM.
//*
//*             THE FOLLOWING MANUAL TAILORING IS REQUIRED:
//*             1. ADD A VALID JOB CARD.
//*             2. LOCATE AND CHANGE ALL OCCURRENCES OF THE FOLLOWING
//*                STRINGS AS INDICATED:
//*                A. '!DSN!' TO THE NAME OF THE DB2 SUBSYSTEM YOU ARE
//*                   PREPARING TO MIGRATE.
//*                B. 'DSN!!0' TO THE PREFIX OF THE SDSNLOAD TARGET
//*                   LIBRARY NAME FOR THE DB2 SUBSYSTEM YOU ARE
//*                   PREPARING TO MIGRATE.
//*                C. 'DSNTIB!!' TO THE PLAN NAME FOR DSNTIAUL ON THE
//*                   DB2 SUBSYSTEM YOU ARE PREPARING TO MIGRATE.
//*
//* CHANGE ACTIVITY =
//**********************************************************************
//*
//JOBLIB  DD  DSN=SDB6R.SDSNLOAD,DISP=SHR
//*
//*        DELETE DATA SETS FROM ANY PREVIOUS RUN
//*
//DELETE  EXEC PGM=IEFBR14
//SYSREC00 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC00,
//            DISP=(MOD,DELETE),
//            UNIT=SYSDA,SPACE=(TRK,0)
//SYSREC01 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC01,
//            DISP=(MOD,DELETE),
//            UNIT=SYSDA,SPACE=(TRK,0)
//SYSREC02 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC02,
//            DISP=(MOD,DELETE),
//            UNIT=SYSDA,SPACE=(TRK,0)
//SYSREC03 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC03,
//            DISP=(MOD,DELETE),
//            UNIT=SYSDA,SPACE=(TRK,0)
//SYSREC04 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC04,
//            DISP=(MOD,DELETE),
//            UNIT=SYSDA,SPACE=(TRK,0)
//SYSREC05 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC05,
//            DISP=(MOD,DELETE),
//            UNIT=SYSDA,SPACE=(TRK,0)
//SYSREC06 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC06,
//            DISP=(MOD,DELETE),
//            UNIT=SYSDA,SPACE=(TRK,0)
//SYSREC07 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC07,
//            DISP=(MOD,DELETE),
//            UNIT=SYSDA,SPACE=(TRK,0)
//SYSREC08 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC08,
//            DISP=(MOD,DELETE),
//            UNIT=SYSDA,SPACE=(TRK,0)
//SYSREC09 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC09,
//            DISP=(MOD,DELETE),
//            UNIT=SYSDA,SPACE=(TRK,0)
//SYSREC10 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC10,
//            DISP=(MOD,DELETE),
//            UNIT=SYSDA,SPACE=(TRK,0)
//SYSREC11 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC11,
//            DISP=(MOD,DELETE),
//            UNIT=SYSDA,SPACE=(TRK,0)
//SYSREC12 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC12,
//            DISP=(MOD,DELETE),
//            UNIT=SYSDA,SPACE=(TRK,0)
//SYSREC13 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC13,
//            DISP=(MOD,DELETE),
//            UNIT=SYSDA,SPACE=(TRK,0)
//SYSREC14 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC14,
//            DISP=(MOD,DELETE),
//            UNIT=SYSDA,SPACE=(TRK,0)
//SYSREC15 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC15,
//            DISP=(MOD,DELETE),
//            UNIT=SYSDA,SPACE=(TRK,0)
//SYSREC16 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC16,
//            DISP=(MOD,DELETE),
//            UNIT=SYSDA,SPACE=(TRK,0)
//SYSREC17 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC17,
//            DISP=(MOD,DELETE),
//            UNIT=SYSDA,SPACE=(TRK,0)
//SYSREC18 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC18,
//            DISP=(MOD,DELETE),
//            UNIT=SYSDA,SPACE=(TRK,0)
//SYSREC19 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC19,
//            DISP=(MOD,DELETE),
//            UNIT=SYSDA,SPACE=(TRK,0)
//*
//*        RUN PREMIGRATION QUERIES
//*
//UNLOAD  EXEC PGM=IKJEFT01,DYNAMNBR=20,COND=(0,NE)
//SYSTSPRT DD  SYSOUT=*
//SYSTSIN  DD  *
 DSN SYSTEM(DB6R)
 RUN  PROGRAM(DSNTIAUL)  PARMS('SQL') -
      LIB('SSXDP.DB6R.RUNLIB.LOAD')
//SYSPRINT DD SYSOUT=*
//SYSUDUMP DD SYSOUT=*
//SYSPUNCH DD DUMMY
//SYSREC00 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC00,
//            DISP=(,CATLG),
//            UNIT=SYSDA,SPACE=(1024,(10,10),RLSE),
//            DCB=(RECFM=FB,LRECL=80)
//SYSREC01 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC01,
//            DISP=(,CATLG),
//            UNIT=SYSDA,SPACE=(1024,(10,10),RLSE),
//            DCB=(RECFM=FB,LRECL=80)
//SYSREC02 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC02,
//            DISP=(,CATLG),
//            UNIT=SYSDA,SPACE=(1024,(10,10),RLSE),
//            DCB=(RECFM=FB,LRECL=80)
//SYSREC03 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC03,
//            DISP=(,CATLG),
//            UNIT=SYSDA,SPACE=(1024,(10,10),RLSE),
//            DCB=(RECFM=FB,LRECL=80)
//SYSREC04 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC04,
//            DISP=(,CATLG),
//            UNIT=SYSDA,SPACE=(1024,(10,10),RLSE),
//            DCB=(RECFM=FB,LRECL=80)
//SYSREC05 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC05,
//            DISP=(,CATLG),
//            UNIT=SYSDA,SPACE=(1024,(10,10),RLSE),
//            DCB=(RECFM=FB,LRECL=80)
//SYSREC06 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC06,
//            DISP=(,CATLG),
//            UNIT=SYSDA,SPACE=(1024,(10,10),RLSE),
//            DCB=(RECFM=FB,LRECL=80)
//SYSREC07 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC07,
//            DISP=(,CATLG),
//            UNIT=SYSDA,SPACE=(1024,(10,10),RLSE),
//            DCB=(RECFM=FB,LRECL=80)
//SYSREC08 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC08,
//            DISP=(,CATLG),
//            UNIT=SYSDA,SPACE=(1024,(10,10),RLSE),
//            DCB=(RECFM=FB,LRECL=80)
//SYSREC09 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC09,
//            DISP=(,CATLG),
//            UNIT=SYSDA,SPACE=(1024,(10,10),RLSE),
//            DCB=(RECFM=FB,LRECL=80)
//SYSREC10 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC10,
//            DISP=(,CATLG),
//            UNIT=SYSDA,SPACE=(1024,(10,10),RLSE),
//            DCB=(RECFM=FB,LRECL=80)
//SYSREC11 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC11,
//            DISP=(,CATLG),
//            UNIT=SYSDA,SPACE=(1024,(10,10),RLSE),
//            DCB=(RECFM=FB,LRECL=80)
//SYSREC12 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC12,
//            DISP=(,CATLG),
//            UNIT=SYSDA,SPACE=(1024,(10,10),RLSE),
//            DCB=(RECFM=FB,LRECL=80)
//SYSREC13 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC13,
//            DISP=(,CATLG),
//            UNIT=SYSDA,SPACE=(1024,(10,10),RLSE),
//            DCB=(RECFM=FB,LRECL=80)
//SYSREC14 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC14,
//            DISP=(,CATLG),
//            UNIT=SYSDA,SPACE=(1024,(10,10),RLSE),
//            DCB=(RECFM=FB,LRECL=80)
//SYSREC15 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC15,
//            DISP=(,CATLG),
//            UNIT=SYSDA,SPACE=(1024,(10,10),RLSE),
//            DCB=(RECFM=FB,LRECL=80)
//SYSREC16 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC16,
//            DISP=(,CATLG),
//            UNIT=SYSDA,SPACE=(1024,(10,10),RLSE),
//            DCB=(RECFM=FB,LRECL=80)
//SYSREC17 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC17,
//            DISP=(,CATLG),
//            UNIT=SYSDA,SPACE=(1024,(10,10),RLSE),
//            DCB=(RECFM=FB,LRECL=80)
//SYSREC18 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC18,
//            DISP=(,CATLG),
//            UNIT=SYSDA,SPACE=(1024,(10,10),RLSE),
//            DCB=(RECFM=FB,LRECL=80)
//SYSREC19 DD DSN=SSXDP.DB2V9.RSU1309.DSNTIJPM.SYSREC19,
//            DISP=(,CATLG),
//            UNIT=SYSDA,SPACE=(1024,(10,10),RLSE),
//            DCB=(RECFM=FB,LRECL=80)
//SYSIN    DD *
  SET CURRENT SQLID='LSEBDB2';
    SELECT CURRENT TIMESTAMP
      FROM SYSIBM.SYSDUMMY1;

  SELECT CHAR( '"' || SUBSTR(NAME, 1, 8) || '"' )
    FROM SYSIBM.SYSDATABASE
   WHERE NAME = 'DSN8D81A';

  SELECT CHAR( '"' || RTRIM(SUBSTR(A.NAME, 1, 18))   || '"' )
        ,CHAR( '"' || RTRIM(SUBSTR(A.TBNAME, 1, 18)) || '"' )
        ,CHAR( '"' || RTRIM(B.TSNAME) || '"' )
    FROM SYSIBM.SYSINDEXES   A,
         SYSIBM.SYSTABLES    B,
         SYSIBM.SYSINDEXPART C
   WHERE A.TBNAME    = B.NAME
     AND A.TBCREATOR = B.CREATOR
     AND A.NAME      = C.IXNAME
     AND A.CREATOR   = C.IXCREATOR
     AND B.DBNAME    = 'DSNDB06'
     AND B.CREATEDBY = 'SYSIBM'
     AND (A.OBID BETWEEN 400 AND 599 OR
          A.OBID BETWEEN 900 AND 1699)
     AND C.STORTYPE  = 'E'
     AND B.TSNAME IN( 'SYSOBJ'  , 'SYSPKAGE' )
  ORDER BY B.TSNAME, A.TBNAME, A.NAME;

  SELECT CHAR( '"' || RTRIM(SUBSTR(A.NAME, 1, 18))   || '"' )
        ,CHAR( '"' || RTRIM(SUBSTR(A.TBNAME, 1, 18)) || '"' )
        ,CHAR( '"' || RTRIM(B.TSNAME) || '"' )
    FROM SYSIBM.SYSINDEXES   A,
         SYSIBM.SYSTABLES    B,
         SYSIBM.SYSINDEXPART C
   WHERE A.TBNAME    = B.NAME
     AND A.TBCREATOR = B.CREATOR
     AND A.NAME      = C.IXNAME
     AND A.CREATOR   = C.IXCREATOR
     AND B.DBNAME    = 'DSNDB06'
     AND B.CREATEDBY = 'SYSIBM'
     AND (A.OBID BETWEEN 400 AND 599 OR
          A.OBID BETWEEN 900 AND 1699)
     AND C.STORTYPE  = 'I'
     AND B.TSNAME IN( 'SYSOBJ'  , 'SYSPKAGE' )
  ORDER BY B.TSNAME, A.TBNAME, A.NAME;

    SELECT CHAR( '"' || RTRIM(SUBSTR(SCHEMA, 1, 8)) || '"' )
          ,CHAR( '"' || RTRIM(SUBSTR(NAME, 1, 18))  || '"' )
      FROM SYSIBM.SYSROUTINES
     WHERE ROUTINETYPE = 'P'
       AND WLM_ENVIRONMENT = ' '
  ORDER BY SCHEMA, NAME;

    SELECT CHAR( '"' || RTRIM(SUBSTR(NAME, 1, 8))  || '"' )
          ,CHAR( '"' || RTRIM(SUBSTR(CREATOR, 1, 8)) || '"' )
          ,BOUNDTS
          ,VALID
          ,OPERATIVE
      FROM SYSIBM.SYSPLAN
     WHERE RELBOUND = ' '
  ORDER BY BOUNDTS;

    SELECT SUBSTR(LOCATION, 1, 16)
          ,CHAR( '"' || RTRIM(SUBSTR(COLLID, 1, 18)) || '"' )
          ,CHAR( '"' || RTRIM(SUBSTR(NAME, 1, 8))  || '"' )
          ,CHAR( '"' || RTRIM(SUBSTR(CREATOR, 1, 8)) || '"' )
          ,SMALLINT(LENGTH(VERSION))
          ,SUBSTR(VERSION
               || '                             '
               || '                             '
                 , 1, 122)
          ,BINDTIME
          ,VALID
          ,OPERATIVE
      FROM SYSIBM.SYSPACKAGE
     WHERE RELBOUND = ' '
  ORDER BY BINDTIME;

    SELECT CHAR( '"' || RTRIM(SUBSTR(SCHEMA, 1, 8)) || '"' )
          ,CHAR( '"' || RTRIM(SUBSTR(NAME, 1, 18)) || '"' )
     FROM SYSIBM.SYSROUTINES
    WHERE EXTERNAL_NAME = 'DSNWZPR'
 ORDER BY SCHEMA, NAME;

   SELECT 'REPORT08 HAS BEEN DISCONTINUED'
     FROM SYSIBM.SYSDUMMY1;

   SELECT NAME, CREATOR, TABLESTATUS
     FROM SYSIBM.SYSTABLES
    WHERE STATUS = 'I';

   SELECT NAME, TBNAME, TBCREATOR
     FROM SYSIBM.SYSCOLUMNS
    WHERE COLSTATUS = 'I';

   SELECT SUBSTR(TB.NAME, 1, 15) AS TBNAME
        , TB.DBNAME
        , TB.TSNAME
        , TB.CREATOR
     FROM SYSIBM.SYSTABLES  TB
        , SYSIBM.SYSCOLUMNS CO
    WHERE TB.NAME    = 'DSN_PTASK_TABLE'
      AND TB.CREATOR = CO.TBCREATOR
      AND TB.NAME    = CO.TBNAME
      AND CO.COLNO IN( 13, 14, 15, 16 )
      AND LENGTH(CO.NAME) > 7
 GROUP BY TB.CREATOR, TB.DBNAME, TB.TSNAME, TB.NAME;

   SELECT SUBSTR(TB.NAME, 1, 15)
        , TB.CREATOR
     FROM SYSIBM.SYSTABLES  TB
        , SYSIBM.SYSCOLUMNS CO
    WHERE TB.NAME    = 'DSN_PTASK_TABLE'
      AND TB.CREATOR = CO.TBCREATOR
      AND TB.NAME    = CO.TBNAME
      AND CO.COLNO IN( 13, 14, 15, 16 )
      AND LENGTH(CO.NAME) = 7
 GROUP BY TB.CREATOR, TB.NAME;

  SELECT STRIP(CREATOR) || '.' || STRIP(NAME)
    FROM SYSIBM.SYSTABLES
   WHERE CREATOR     = 'SYSIBM'
     AND NAME        = 'SYSROUTINES'
     AND DATACAPTURE = 'Y';

  SELECT STRIP(CREATOR) || '.' || STRIP(NAME)
    FROM SYSIBM.SYSTABLES
   WHERE CREATOR     = 'SYSIBM'
     AND NAME        = 'SYSPACKSTMT'
     AND DATACAPTURE = 'Y';

   SELECT STRIP(DCREATOR) || '.' || STRIP(DNAME)
     FROM SYSIBM.SYSVIEWDEP,
        ( SELECT CREATOR, NAME
            FROM SYSIBM.SYSTABLES
           WHERE NUM_DEP_MQTS > 0
             AND NAME = 'SYSROUTINES'
        )  AS STB
    WHERE BNAME = STB.NAME
  ;

   SELECT STRIP(DCREATOR) || '.' || STRIP(DNAME)
     FROM SYSIBM.SYSVIEWDEP,
        ( SELECT CREATOR, NAME
            FROM SYSIBM.SYSTABLES
           WHERE NUM_DEP_MQTS > 0
             AND NAME = 'SYSPACKSTMT'
        ) AS STB
    WHERE BNAME = STB.NAME
  ;

   SELECT STRIP(TB.CREATOR) || '.' || STRIP(TB.NAME)
     FROM SYSIBM.SYSTABLES     TB
         ,SYSIBM.SYSTABLESPACE TS
    WHERE TB.DBNAME = TS.DBNAME
      AND TB.TSNAME = TS.NAME
      AND TB.TYPE = 'T'
      AND TS.ENCODING_SCHEME <> 'U'
      AND TB.NAME IN( 'DSN_DETCOST_TABLE'
                     ,'DSN_FILTER_TABLE'
                     ,'DSN_FUNCTION_TABLE'
                     ,'DSN_PGRANGE_TABLE'
                     ,'DSN_PGROUP_TABLE'
                     ,'DSN_PREDICAT_TABLE'
                     ,'DSN_PROFILE_TABLE'
                     ,'DSN_PROFILE_HISTORY'
                     ,'DSN_PTASK_TABLE'
                     ,'DSN_QUERY_AUX'
                     ,'DSN_QUERY_TABLE'
                     ,'DSN_SORTKEY_TABLE'
                     ,'DSN_SORT_TABLE'
                     ,'DSN_STATEMENT_CACHE_AUX'
                     ,'DSN_STATEMENT_CACHE_TABLE'
                     ,'DSN_STATEMENT_RUNTIME_INFO'
                     ,'DSN_STATEMNT_TABLE'
                     ,'DSN_STRUCT_TABLE'
                     ,'DSN_VIEWREF_TABLE'
                     ,'DSN_VIRTUAL_INDEX'
                     ,'PLAN_TABLE'
                     )
   ORDER BY TB.CREATOR, TB.NAME
  ;

  SELECT DISTINCT CHAR( SUBSTR( STRIP(TBCREATOR) || '.' || STRIP(TBNAME)
                             || REPEAT( ' ', 255 ), 1, 255 )
                      )
    FROM SYSIBM.SYSCOLUMNS
   WHERE STRIP(TBCREATOR) || STRIP(TBNAME)
      IN ( SELECT STRIP(TBCREATOR) || STRIP(TBNAME)
             FROM SYSIBM.SYSCOLUMNS
            WHERE TBNAME IN( 'DSN_DETCOST_TABLE'
                           , 'DSN_FILTER_TABLE'
                           , 'DSN_FUNCTION_TABLE'
                           , 'DSN_PGRANGE_TABLE'
                           , 'DSN_PGROUP_TABLE'
                           , 'DSN_PREDICAT_TABLE'
                           , 'DSN_PROFILE_HISTORY'
                           , 'DSN_PROFILE_TABLE'
                           , 'DSN_PTASK_TABLE'
                           , 'DSN_QUERY_TABLE'
                           , 'DSN_SORT_TABLE'
                           , 'DSN_SORTKEY_TABLE'
                           , 'DSN_STATEMENT_CACHE_TABLE'
                           , 'DSN_STATEMENT_RUNTIME_INFO'
                           , 'DSN_STATEMNT_TABLE'
                           , 'DSN_STRUCT_TABLE'
                           , 'DSN_VIEWREF_TABLE'
                           , 'DSN_VIRTUAL_INDEXES'
                           , 'PLAN_TABLE'
                           )
         GROUP BY TBCREATOR, TBNAME
           HAVING (TBNAME = 'DSN_DETCOST_TABLE'
                   AND MAX(COLNO) < 120)
               OR (TBNAME = 'DSN_FILTER_TABLE'
                   AND MAX(COLNO) <  14)
               OR (TBNAME = 'DSN_FUNCTION_TABLE'
                   AND MAX(COLNO) <  15)
               OR (TBNAME = 'DSN_PGRANGE_TABLE'
                   AND MAX(COLNO) <   9)
               OR (TBNAME = 'DSN_PGROUP_TABLE'
                   AND MAX(COLNO) <  38)
               OR (TBNAME = 'DSN_PREDICAT_TABLE'
                   AND MAX(COLNO) <  36)
               OR (TBNAME = 'DSN_PROFILE_HISTORY'
                   AND MAX(COLNO) <  10)
               OR (TBNAME = 'DSN_PROFILE_TABLE'
                   AND MAX(COLNO) <  10)
               OR (TBNAME = 'DSN_PTASK_TABLE'
                   AND MAX(COLNO) <  21)
               OR (TBNAME = 'DSN_QUERY_TABLE'
                   AND MAX(COLNO) <  10)
               OR (TBNAME = 'DSN_SORT_TABLE'
                   AND MAX(COLNO) <  13)
               OR (TBNAME = 'DSN_SORTKEY_TABLE'
                   AND MAX(COLNO) <  18)
               OR (TBNAME = 'DSN_STATEMENT_CACHE_TABLE'
                   AND MAX(COLNO) <  48)
               OR (TBNAME = 'DSN_STATEMENT_RUNTIME_INFO'
                   AND MAX(COLNO) <  51)
               OR (TBNAME = 'DSN_STATEMNT_TABLE'
                   AND MAX(COLNO) <  13)
               OR (TBNAME = 'DSN_STRUCT_TABLE'
                   AND MAX(COLNO) <  15)
               OR (TBNAME = 'DSN_VIEWREF_TABLE'
                   AND MAX(COLNO) <  11)
               OR (TBNAME = 'DSN_VIRTUAL_INDEXES'
                   AND MAX(COLNO) < 145)
               OR (TBNAME = 'PLAN_TABLE'
                   AND MAX(COLNO) <  59)
           UNION
           SELECT STRIP(TBCREATOR) || STRIP(TBNAME)
             FROM SYSIBM.SYSCOLUMNS
            WHERE TBNAME IN( 'DSN_DETCOST_TABLE'
                           , 'DSN_FILTER_TABLE'
                           , 'DSN_FUNCTION_TABLE'
                           , 'DSN_PGRANGE_TABLE'
                           , 'DSN_PGROUP_TABLE'
                           , 'DSN_PREDICAT_TABLE'
                           , 'DSN_PROFILE_HISTORY'
                           , 'DSN_PROFILE_TABLE'
                           , 'DSN_PTASK_TABLE'
                           , 'DSN_QUERY_TABLE'
                           , 'DSN_SORT_TABLE'
                           , 'DSN_SORTKEY_TABLE'
                           , 'DSN_STATEMENT_CACHE_TABLE'
                           , 'DSN_STATEMENT_RUNTIME_INFO'
                           , 'DSN_STATEMNT_TABLE'
                           , 'DSN_STRUCT_TABLE'
                           , 'DSN_VIEWREF_TABLE'
                           , 'DSN_VIRTUAL_INDEXES'
                           , 'PLAN_TABLE'
                           )
              AND (    (NAME = 'ACCESSCREATOR'    AND LENGTH <> 128)
                    OR (NAME = 'ACCESSNAME'       AND LENGTH <> 128)
                    OR (NAME = 'APPLNAME'         AND LENGTH <> 24)
                    OR (NAME = 'COLLID'           AND LENGTH <> 128)
                    OR (NAME = 'CORRELATION_NAME' AND LENGTH <> 128)
                    OR (NAME = 'CREATOR'          AND LENGTH <> 128)
                    OR (NAME = 'FUNCTION_NAME'    AND LENGTH <> 128)
                    OR (NAME = 'FUNCTION_TEXT'    AND LENGTH <> 1500)
                    OR (NAME = 'GROUP_MEMBER'     AND LENGTH <> 24)
                    OR (NAME = 'HINTUSED'         AND LENGTH <> 128)
                    OR (NAME = 'OPTHINT'          AND LENGTH <> 128)
                    OR (NAME = 'PATH'             AND LENGTH <> 2048)
                    OR (NAME = 'PLANNAME'         AND LENGTH <> 24)
                    OR (NAME = 'PROGNAME'         AND LENGTH <> 128)
                    OR (NAME = 'REMARKS'          AND LENGTH <> 762)
                    OR (NAME = 'SCHEMA_NAME'      AND LENGTH <> 128)
                    OR (NAME = 'SPEC_FUNC_NAME'   AND LENGTH <> 128)
                    OR (NAME = 'TNAME'            AND LENGTH <> 128)
                    OR (NAME = 'VERSION'          AND LENGTH <> 122)
                    OR (NAME = 'VIEW_CREATOR'     AND LENGTH <> 128)
                    OR (NAME = 'VIEW_NAME'        AND LENGTH <> 128)
                  )
         )
  ;

  SELECT CHAR( SUBSTR( STRIP(SUBSTR(CREATOR, 1, 8))
                    || '.'
                    || STRIP(SUBSTR(NAME, 1, 8))
                    || '.'
                    || STRIP(CHAR(OLDEST_VERSION))
                    || '.'
                    || STRIP(CHAR(CURRENT_VERSION))
                    || REPEAT( ' ', 28 )
                     , 1, 28
                     )
             )
    FROM SYSIBM.SYSTABLESPACE
   WHERE DBID = 6
     AND (CURRENT_VERSION < OLDEST_VERSION);

/*
//*
//*********************************************************************
//*        Report existence of the V8 sample database
//*********************************************************************
//REPORT01 EXEC PGM=IEBGENER,COND=(4,LT)
