 SET CURRENT SQLID='SYSIBM';
 CREATE UNIQUE INDEX SYSIBM.INDEXSPACESTATS_IX
   ON SYSIBM.INDEXSPACESTATS
    (DBID                  ASC,
     "ISOBID"              ASC,
     PARTITION             ASC)
   USING STOGROUP SYSDEFLT
   PRIQTY 12 SECQTY 12
   ERASE  NO
   FREEPAGE 0 PCTFREE 10
   GBPCACHE CHANGED
   CLUSTER
   BUFFERPOOL BP3
   CLOSE NO
      COPY NO
      PIECESIZE 2 G;
    COMMIT;
    SET CURRENT SQLID='DT0753E';
    CREATE DATABASE DSNRTSDB
      BUFFERPOOL BP2
      INDEXBP    BP3
      CCSID      EBCDIC
      STOGROUP   SYSDEFLT;
    COMMIT;
