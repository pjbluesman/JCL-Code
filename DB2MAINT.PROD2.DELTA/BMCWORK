-- DMRPRDB1.ABDTINDX WILL BE CREATED VIA THESE NATIVE DB2 COMMANDS.

    SET CURRENT SQLID = 'SYSPRO';

    CREATE        UNIQUE INDEX DMRPRDB1.ABDTINDX
           ON DMRPR.DMRABDTL
          (DATE                 ASC
          ,LOCATION             ASC
          ,SUBSYSTEM            ASC
          ,PLANNAME             ASC
          ,AUTHID               ASC
          ,BPNAME               ASC
          ,DATETIME             ASC
          ,IFCIDSEQ#            ASC
          ,"CONNECTION"         ASC
          ,CORRID               ASC
          ,ORIGPRIMID           ASC
          )
                     USING STOGROUP DMRPRSG1
                                    PRIQTY 30240
                                    SECQTY 720
                                    ERASE NO
                     FREEPAGE 0
                     PCTFREE 10
           BUFFERPOOL BP2
           CLOSE NO
           PIECESIZE 2097152K
    ;

