.SYNC 110       'CREATE TABLE DMRPR.DMRACSM2'
    SET CURRENT SQLID = USER;


-- DMRPR.DMRACSUM WILL BE CREATED VIA THESE NATIVE DB2 COMMANDS.

    SET CURRENT SQLID = 'SYSPRO';

