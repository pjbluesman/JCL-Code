.CONTROL SN(DT0072,DMRPRDB1)

--==============================================================================
-- VRCM6.1.5    CA-DB2   RC/MIGRATOR   ANALYSIS REPORT      11-25-02 14:57
--==============================================================================
--STRATEGY INFORMATION:
--STRATEGY ==> DMRPRDB1  DESCRIPTION  ===> MIGRATE
--CREATOR  ==> DT0072    SHARE OPTION ===> N (U,Y,N,X,L)  SRC SSID ===> DB2R
--
--IMPLODE/EXPLODE OPTIONS:
--  +-- IMPLODE --+ +- PRIMARY OBJECT -+ +---------- EXPLODE OPTIONS ----------+
--S     STOGROUP      DATABASE  CREATOR  TBLSPACE  TABLE INDEX VIEW SYN/AL TRIG
--
--
--OTHER MIGRATION OPTIONS:
--   +- PRIMARY OBJECT -+   AUX                  ALL   NUMBER
--S   DATABASE  CREATOR   IMPLODE  RI  SECURITY  ROWS  OF ROWS
--
--ANALYSIS OPTIONS:
--  EXECUTION MODE    ===> O (O - ONLINE, B - BATCH)
--  ANALYSIS OVERRIDE ===> N
--  ACCESS MODE       ===> CATALOG
--  MODEL ID          ===> @DEFAULT
--  MODEL CREATOR     ===> DT0072
--  TARGET SSID       ===> DB3V
--  GLOBAL CHANGES    ===> N
--
--  AUX IMPLODE       ===> N             STATISTICS    ===> N
--  RI(LOCAL/GLOBAL)  ===> L             CHECK DATA    ===> N
--  SECURITY          ===> Y             IMAGE COPY    ===> N
--  DROP OBJECTS      ===> N             BIND/REBIND   ===> B
--  DEFAULT SQLID     ===> SYSPRO
--                                       RECOVER IX    ===> N
--
--  ALL ROWS          ===> N             VSAM DEFINES  ===> N
--  NUMBER ROWS       ===>               VSAM DELETES  ===> N
--  DATA STATISTICS   ===> N
--
--  NO .AUTHS               ===> Y
--  NO SET SQLID STMTS      ===> N
--  BND/DAT/SQL/STAT/GRNT/RI===> N  (B nd, D at, R -Stat, S QL, G rnt, R I or N)
--  COMMIT ASAP             ===> N
--
--  DELETE UNLOAD DATASETS    ===> N
--  DELETE TEMPORARY DATASETS ===> N
--
--********************** END OF ANALYSIS HEADER ********************************

--RMA233W NO UNLOADS HAVE BEEN GENERATED DURING THIS ANALYSIS.

.SYSTEM SQLDDL
.CONNECT DB2R

-- **************************************************************
-- *                                                            *
-- * DATABASE CREATE AND ALTER STATEMENTS                       *
-- *                                                            *
-- **************************************************************


-- DMRPRDB1 WILL BE CREATED VIA THESE NATIVE DB2 COMMANDS.
    COMMIT  ;

.SYNC 1         'CREATE STORAGE GROUP DMRPRSG1'

    SET CURRENT SQLID = 'SYSPRO';
    DROP   DATABASE DMRPRDB1  ;
    COMMIT  ;


.SYNC 2         'CREATE STORAGE GROUP DMRPRSG1'

    CREATE DATABASE DMRPRDB1
           STOGROUP DMRPRSG1
           BUFFERPOOL BP3
           INDEXBP    BP2
           CCSID EBCDIC
    ;

.SYNC 5         'CREATE DATABASE DMRPRDB1'
    SET CURRENT SQLID = USER;


    SET CURRENT SQLID = 'SYSPRO';

    GRANT DBADM ON DATABASE DMRPRDB1 TO SUB;

.SYNC 10        'GRANT DATABASE SYSPRO.DMRPRDB1'

    SET CURRENT SQLID = USER;

-- **************************************************************
-- *                                                            *
-- * TABLESPACE CREATE AND ALTER STATEMENTS                     *
-- *                                                            *
-- **************************************************************


-- DMRPRDB1.DMRPRTAD WILL BE CREATED VIA THESE NATIVE DB2 COMMANDS.

    SET CURRENT SQLID = 'SYSPRO';

    CREATE TABLESPACE DMRPRTAD
           IN DMRPRDB1
                     USING STOGROUP DMRPRSG1
                                    PRIQTY 216000
                                    SECQTY 7200
                                    ERASE NO
                     FREEPAGE 0
                     PCTFREE 5
                     COMPRESS YES
           BUFFERPOOL BP3
           LOCKSIZE ANY
           SEGSIZE 32
           CLOSE NO
           LOCKMAX SYSTEM
           CCSID EBCDIC
    ;

