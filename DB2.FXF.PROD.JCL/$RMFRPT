//EXITLIB   DD DISP=SHR,DSN=SYS1.SERBLINK
//SYSOUT    DD SYSOUT=*
//*//SORTIN    DD DISP=SHR,DSN=AFWP.SMF.AFWC.WEEKLY(0)
//*//          DD DISP=SHR,DSN=AFWP.SMF.AFW1.WEEKLY(0)
//*//          DD DISP=SHR,DSN=AFWP.SMF.AFW2.WEEKLY(0)
