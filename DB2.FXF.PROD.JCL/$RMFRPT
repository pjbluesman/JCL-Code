//SYSIN     DD *
   INCLUDE COND=((06,01,GE,70,AND,
                  06,01,LE,79),AND,
                 (12,3,PD,GE,10201,AND,     * <- JULIAN START
                  12,3,PD,LE,10201)),       * <- JULIAN END
                 FORMAT=FI
   SORT FIELDS=(11,4,CH,A,7,4,CH,A),EQUALS,SIZE=E800000
   MODS E15=(ERBPPE15,36000,,N),E35=(ERBPPE35,3000,,N)
