//JS1      EXEC DSNUPROC,SYSTEM=DSNB,UID=DBAUNLOAD                      00110000
//SREC     DD  SYSOUT=*                                                 00120000
//SPUNCH   DD  SYSOUT=*                                                 00130000
//SYSIN    DD  DISP=SHR,DSN=AFDB.PROD.PARMLIB(OPTNSKIP)                 00140000
