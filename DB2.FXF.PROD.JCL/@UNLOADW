//         DD  DISP=SHR,DSN=AFDB.PROD.TEMPLATE(RPICTAPE)                00160000
//         DD  *                                                        00170000
TEMPLATE PUNCHDSN                                                       00171000
         DSN      (AFDB.&DB..&SN..D&DT..T&TI..PUN)                      00172000
         DISP     (NEW,CATLG,CATLG)                                     00173000
         UNIT      SYSDA                                                00174000
         VOLCNT    15                                                   00175000
         SPACE    (1,1) CYL                                             00176000
TEMPLATE UNLOADSN                                                       00177000
         DSN      (AFDB.&DB..&SN..D&DT..T&TI..DAT)                      00178000
         DISP     (NEW,CATLG,CATLG)                                     00179000
         UNIT      SYSDA                                                00179100
         VOLCNT    15                                                   00179200
         SPACE    (10,10) CYL                                           00179300
                                                                        00179600
UNLOAD TABLESPACE ISHFSYS.TFBSH                                         00230000
       FROMCOPY DB2T.ISHFSYS.TFBSH.P00000.FLP.D1310500                  00240000
       UNLDDN UNLOADSN                                                  00250000
       PUNCHDDN PUNCHDSN                                                00260000
       SHRLEVEL REFERENCE                                               00270000
   FROM TABLE DB2FFI.SHIPMENT                                           00280000
       WHEN (PRO_NUMBER IN ('400101274199',                             00290000
