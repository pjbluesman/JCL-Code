//SYSTSIN  DD  *
  DSN SYSTEM(DB4V)
  -START TRACE(A) CLASS(1,2,3,7,8) AUTHID(RPADAPP,PROD) DEST(GTF)
  -START TRACE(S) CLASS(1,3,4,5,6) DEST(GTF)
  -START TRACE(P) CLASS(30) IFCID(221,222,231) AUTHID(RPADAPP,PROD)
  DEST(GTF)
