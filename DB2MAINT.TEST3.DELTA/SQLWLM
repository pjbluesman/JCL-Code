  SELECT SUBSTR(STRIP(SCHEMA) || '.' || STRIP(NAME),1,40) STORPROC
       , SUBSTR(WLM_ENVIRONMENT,1,15) WLM_ENVIRONMENT
       , SUBSTR(SPECIFICNAME,1,30) SPECIFICNAME
       , SUBSTR(OWNER,1,8) OWNER
       , SUBSTR(LANGUAGE,1,10) LANGUAGE
       , DATE(CREATEDTS) CREATED
       , DATE(ALTEREDTS) ALTERED
       , SUBSTR(EXTERNAL_NAME,1,50) EXTERNAL_NAME
    FROM SYSIBM.SYSROUTINES
   ORDER BY 1,2
    WITH UR;
