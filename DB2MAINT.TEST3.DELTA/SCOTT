   SELECT DISTINCT RLOCR2.TKT_DOC_NB
        , RLOCR2.TKT_DOC_ISS_LDT
        , RLOCR2.TKT_DOC_SQ_NB
        , 'K'
   FROM EBKDP03.ET_TKDC_BKG_RLOCR RLOCR1
      , EBKDP03.ET_TKDC_BKG_RLOCR RLOCR2
      , EBKDP03.ET_TKT_DOC        TKT2
   WHERE RLOCR1.TKT_DOC_ISS_LDT =
      (SELECT MAX(TKT_DOC_ISS_LDT)
       FROM EBKDP03.ET_TKT_DOC
       WHERE TKT_DOC_NB='0062180456536' )
   AND RLOCR1.TKT_DOC_NB='0062180456536'
   AND RLOCR1.TKT_DOC_ISS_LDT = RLOCR2.TKT_DOC_ISS_LDT
   AND RLOCR1.RES_SYS_PVDR_CD = RLOCR2.RES_SYS_PVDR_CD
   AND RLOCR1.BKG_REC_LOCR_TXT = RLOCR2.BKG_REC_LOCR_TXT
   AND RLOCR2.TKT_DOC_NB = TKT2.TKT_DOC_NB
   AND RLOCR2.TKT_DOC_ISS_LDT = TKT2.TKT_DOC_ISS_LDT
   AND RLOCR2.TKT_DOC_SQ_NB = TKT2.TKT_DOC_SQ_NB
   AND TKT2.MT_TKT_DOC_TYP_CD = 'TKT'
   FOR FETCH ONLY WITH UR                     ;
